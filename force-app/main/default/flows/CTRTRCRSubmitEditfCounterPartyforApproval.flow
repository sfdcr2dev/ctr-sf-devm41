    <formulas>
        <name>CheckRecordType</name>
        <dataType>String</dataType>
        <expression>IF(contains({!GetRequestItemRecord.RecordTypeName__c}, &apos;Customer&apos;), TEXT({!GetRequestItemRecord.SalesOrganization__c}), IF(contains({!GetRequestItemRecord.RecordTypeName__c}, &apos;Supplier&apos;),TEXT({!GetRequestItemRecord.SalesOrganization__c}),&apos;&apos;))</expression>
    </formulas>
    <formulas>
        <name>fmlCountry</name>
        <dataType>String</dataType>
        <expression>IF({!GetRequestItemRecord.CTRRequestFormHeader__r.Country__r.Code__c}=&quot;TH&quot;, &quot;Domestic&quot;, &quot;International&quot;)</expression>
    </formulas>
    <formulas>
        <name>FmlNotifybody</name>
        <dataType>String</dataType>
        <expression>{!GetRequestItemRecord.Name} +&quot; &quot;+ &quot;is submitted to you. Please provide your approval to proceed further.&quot;</expression>
    </formulas>
    <formulas>
        <name>FmlNotifySubject</name>
        <dataType>String</dataType>
        <expression>&quot;Request Number&quot;+&quot; &quot;+{!GetRequestItemRecord.Name}+&quot; &quot;+&quot; is submitted to you. Please provide your approval to proceed further.&quot;</expression>
    </formulas>
    <formulas>
        <name>LABIXEmailTitle</name>
        <dataType>String</dataType>
        <expression>&apos;[FYI]:&apos;+&apos; &apos;+ {!GetRequestItemRecord.CTRRequestFormHeader__r.Customer__r.Name}+&apos; &apos;+ &apos; FALB request to edit information&apos;</expression>
    </formulas>
    <formulas>
        <name>LABIXEmailTitleSH</name>
        <dataType>String</dataType>
        <expression>&apos;[FYA]&apos;+ &apos;[&apos; + {!GetRequestItemRecord.SubBU__c} + &apos;]:&apos; + &apos; FALB request to edit information: &apos; + {!GetRequestItemRecord.CompanyName__c}</expression>
    </formulas>
    <formulas>
        <name>ProcessInstanceLinkFormula</name>
        <dataType>String</dataType>
        <expression>LEFT({!$Api.Partner_Server_URL_260}, FIND( &apos;/services&apos;, {!$Api.Partner_Server_URL_260})) &amp; {!GetProcessInstanceWorkItem.Id}</expression>
    </formulas>
