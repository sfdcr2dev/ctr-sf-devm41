    <decisions>
        <name>checkCurrentUser</name>
        <label>checkCurrentUser</label>
        <locationX>1040</locationX>
        <locationY>242</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>OwnerIdEqualsToCurrentUserId</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>OwnerId15Digits</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>$User.Id</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Lead.Status</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Unqualified</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>CheckCountry</targetReference>
            </connector>
            <label>OwnerId = CurrentUserId</label>
        </rules>
        <rules>
            <name>OwnerIdNotEqualToCurrentUserId</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>OwnerId15Digits</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$User.Id</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>ScreenDoNotAllowToConvert_NotOwner</targetReference>
            </connector>
            <label>OwnerId != CurrentUserId</label>
        </rules>
        <rules>
            <name>UnqualifiedLead</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Lead.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Unqualified</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>ScreenDoNotAllowToConvert_Unqualified</targetReference>
            </connector>
            <label>UnqualifiedLead</label>
        </rules>
    </decisions>
