    <formulas>
        <name>fmlLeadType</name>
        <dataType>String</dataType>
        <expression>if({!recordId.RecordType.Name} = &quot;TxCustomer&quot;, &quot;customer&quot;, if({!recordId.RecordType.Name} = &quot;TxSupplier&quot;,&quot;supplier&quot;, &quot;customer/supplier&quot;))</expression>
    </formulas>
    <formulas>
        <name>OwnerId15Digits</name>
        <dataType>String</dataType>
        <expression>LEFT({!recordId.Owner:User.Id},15)</expression>
    </formulas>
    <formulas>
        <name>recNewAccountLink</name>
        <dataType>String</dataType>
        <expression>LEFT({!$Api.Partner_Server_URL_260}, FIND( &apos;/services&apos;, {!$Api.Partner_Server_URL_260})) &amp; {!ConvertedAccountId}</expression>
    </formulas>
    <formulas>
        <name>recNewReqLink</name>
        <dataType>String</dataType>
        <expression>LEFT({!$Api.Partner_Server_URL_260}, FIND( &apos;/services&apos;, {!$Api.Partner_Server_URL_260})) &amp; {!newRequestHeaderId}</expression>
    </formulas>
