    <decisions>
        <name>SHPrescreenDecision</name>
        <label>SHPrescreenDecision</label>
        <locationX>314</locationX>
        <locationY>971</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Pass</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.SHPrescreenDecision__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Pass</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.SHPrescreenDecision__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record__Prior.SHPrescreenDecision__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>UpdatePassRecord</targetReference>
            </connector>
            <label>Pass</label>
        </rules>
        <rules>
            <name>NotPass</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.SHPrescreenDecision__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Not Pass</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.SHPrescreenDecision__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record__Prior.SHPrescreenDecision__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>UpdateNotPassRecord</targetReference>
            </connector>
            <label>NotPass</label>
        </rules>
    </decisions>
    <description>22/02/2024 Edit update element after section head submit result.
- Pass; Change update approval step from &quot;In Review&quot; to &quot;Review Pre-Screen&quot;.
- Not Pass; Add-on update status to &quot;Pre Screen&quot;</description>
    <formulas>
        <description>Subject email</description>
        <name>EmailSubject</name>
        <dataType>String</dataType>
        <expression>&quot;[FYI] Initial: &quot; &amp; {!$Record.CompanyName__c} &amp; &quot; Pre-Screen Result&quot;</expression>
    </formulas>
    <formulas>
        <name>EmailSubjectSupplier</name>
        <dataType>String</dataType>
        <expression>&quot;[FYI] Initial: &amp; $Record.CompanyName__c &amp; Pre-Screen Result from Section Head&quot;</expression>
    </formulas>
    <formulas>
        <name>FMLOrg</name>
        <dataType>String</dataType>
        <expression>IF(contains({!$Record.RecordType.DeveloperName}, &apos;Customer&apos;), TEXT({!$Record.SalesOrganization__c}), IF(contains({!$Record.RecordType.DeveloperName}, &apos;Supplier&apos;),TEXT({!$Record.PurchasingOrganization__c}),&apos;&apos;))</expression>
    </formulas>
    <formulas>
        <name>NotifyBody</name>
        <dataType>String</dataType>
        <expression>&quot;Please review pre-screened by Section Head.&quot;</expression>
    </formulas>
    <formulas>
        <name>NotifyTitle</name>
        <dataType>String</dataType>
        <expression>{!$Record.Customer__r.Name} &amp; &quot; Pre-Screen Result from Section Head&quot;</expression>
    </formulas>
    <formulas>
        <name>RecordLinkFormula</name>
        <dataType>String</dataType>
        <expression>LEFT({!$Api.Partner_Server_URL_260}, FIND( &apos;/services&apos;, {!$Api.Partner_Server_URL_260})) &amp; {!varTargetRecord}</expression>
    </formulas>
