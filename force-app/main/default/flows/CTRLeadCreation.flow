    <textTemplates>
        <name>ctrEmailBody</name>
        <text>&lt;span style=&quot;font-family: sans-serif;&quot;&gt;Dear {!$Record.FirstName} {!$Record.LastName},&lt;/span&gt;&lt;br&gt;&lt;br&gt;&lt;span style=&quot;font-family: sans-serif;&quot;&gt;Thank you for your registration of interested in Thai Oil Group.&lt;/span&gt;&lt;br&gt;&lt;br&gt;&lt;span style=&quot;font-family: sans-serif;&quot;&gt;Best Regards,&lt;/span&gt;&lt;br&gt;&lt;span style=&quot;font-family: sans-serif;&quot;&gt;Registration of Interested Automated System&lt;/span&gt;&lt;br&gt;This is an automatic generated email. Please do not reply to this address.</text>
    </textTemplates>
    <textTemplates>
        <description>Email Body for TOP Trader</description>
        <name>TOPEmailBody</name>
        <text>&lt;span style=&quot;font-family: sans-serif; font-size: 9pt; color: rgb(0, 0, 0);&quot;&gt;Dear Commercial Team,&lt;/span&gt;&lt;br&gt;&lt;br&gt;&lt;span style=&quot;font-family: sans-serif;&quot;&gt;New counterparty wishes to register with the following details,&lt;/span&gt;&lt;br&gt;&lt;span style=&quot;font-family: sans-serif;&quot;&gt;please review and perform next action by visiting&lt;/span&gt;&lt;span style=&quot;font-family: sans-serif; font-size: 9pt; color: rgb(0, 0, 0);&quot;&gt; &lt;/span&gt;&lt;a href=&quot;{!RecordLinkFormula}&quot; rel=&quot;noopener noreferrer&quot; target=&quot;_blank&quot; style=&quot;font-family: sans-serif; font-size: 9pt; color: rgb(0, 0, 0);&quot;&gt;this link&lt;/a&gt;&lt;span style=&quot;font-family: sans-serif; font-size: 9pt; color: rgb(0, 0, 0);&quot;&gt;.&lt;/span&gt;&lt;br&gt;&lt;br&gt;&lt;span style=&quot;font-family: sans-serif; font-size: 9pt; color: rgb(0, 0, 0);&quot;&gt;Company Name: &lt;/span&gt;&lt;strong style=&quot;font-family: sans-serif; font-size: 9pt; color: rgb(0, 0, 0);&quot;&gt;{!$Record.CustomerNameEnglish__c}&lt;/strong&gt;&lt;br&gt;&lt;span style=&quot;font-family: sans-serif; font-size: 9pt; color: rgb(0, 0, 0);&quot;&gt;Interested in registration to be:&lt;/span&gt;&lt;ul&gt;&lt;li&gt;&lt;strong style=&quot;font-family: sans-serif; font-size: 9pt; color: rgb(0, 0, 0);&quot;&gt;Customer: {!fxCustomerProduct}&lt;/strong&gt;&lt;/li&gt;&lt;li&gt;&lt;strong style=&quot;font-family: sans-serif; font-size: 9pt; color: rgb(0, 0, 0);&quot;&gt;Supplier: {!fxSupplierProduct}&lt;/strong&gt;&lt;/li&gt;&lt;/ul&gt;&lt;br&gt;&lt;span style=&quot;font-family: sans-serif; font-size: 9pt; color: rgb(0, 0, 0);&quot;&gt;Best Regards,&lt;/span&gt;&lt;br&gt;Registration of Interested Automated System&lt;br&gt;This is an automatic generated email. Please do not reply to this address.</text>
