    <formulas>
        <name>ctrEmailSubject</name>
        <dataType>String</dataType>
        <expression>&apos;[FYI] Initial: Thank You for Registration with Thai Oil Group&apos;</expression>
    </formulas>
    <formulas>
        <name>fmlType</name>
        <dataType>String</dataType>
        <expression>IF(CONTAINS({!varMySupplier}, &quot;Crude&quot;), &quot;Crude&quot;,
IF({!$Record.Country__r.Code__c} = &quot;TH&quot;, &quot;Domestic&quot;, &quot;International&quot;))</expression>
    </formulas>
    <formulas>
        <name>fxCustomerProduct</name>
        <dataType>String</dataType>
        <expression>IF({!varMyCustomer} = NULL, &apos;-&apos;, {!varMyCustomer})</expression>
    </formulas>
    <formulas>
        <name>fxSupplierProduct</name>
        <dataType>String</dataType>
        <expression>IF(AND({!varMySupplier} = NULL,{!varMySupplierString} = NULL), &apos;-&apos;, IF({!varMySupplier} = NULL, {!varMySupplierString}, IF({!varMySupplierString} = NULL, {!varMySupplier}, {!varMySupplier}&amp;&quot;, &quot;&amp;{!varMySupplierString})))</expression>
    </formulas>
    <formulas>
        <name>getLeftCustomer</name>
        <dataType>String</dataType>
        <expression>LEFT({!varMyCustomerString},FIND(&quot;;&quot;,{!varMyCustomerString})-1)</expression>
    </formulas>
    <formulas>
        <name>getLeftSupplier</name>
        <dataType>String</dataType>
        <expression>LEFT({!varMySupplierString},FIND(&quot;;&quot;,{!varMySupplierString})-1)</expression>
    </formulas>
    <formulas>
        <name>RecordLinkFormula</name>
        <dataType>String</dataType>
        <expression>LEFT({!$Api.Partner_Server_URL_260}, FIND( &apos;/services&apos;, {!$Api.Partner_Server_URL_260})) &amp; {!varTargetRecord}</expression>
    </formulas>
    <formulas>
        <name>removeParsedCustomerValue</name>
        <dataType>String</dataType>
        <expression>TRIM((RIGHT({!varMyCustomerString},LEN({!varMyCustomerString})-FIND(&quot;;&quot;,{!varMyCustomerString}))))</expression>
    </formulas>
    <formulas>
        <name>removeParsedSupplierValue</name>
        <dataType>String</dataType>
        <expression>TRIM((RIGHT({!varMySupplierString},LEN({!varMySupplierString})-FIND(&quot;;&quot;,{!varMySupplierString}))))</expression>
    </formulas>
    <formulas>
        <description>Subject email for TOP</description>
        <name>TOPEmailSubject</name>
        <dataType>String</dataType>
        <expression>&apos;[FYA] Initial: &apos; + {!$Record.Company} + &apos; New Registration Request&apos;</expression>
    </formulas>
