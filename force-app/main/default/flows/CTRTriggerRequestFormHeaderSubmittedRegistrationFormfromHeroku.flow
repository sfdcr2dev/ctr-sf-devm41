        <dataType>String</dataType>
        <expression>&quot;[FYA][&quot; &amp; {!loopRequestFormItem.SubBU__c} &amp; &quot;]: Registration Submission of &quot; &amp; {!$Record.OriginalRequestFormHeader__r.Name}</expression>
    </formulas>
    <formulas>
        <name>NotifyBody</name>
        <dataType>String</dataType>
        <expression>&quot;Please review and consider submitted Registration Information&quot;</expression>
    </formulas>
    <formulas>
