                </rightValue>
            </conditions>
            <connector>
                <targetReference>CompleteAction</targetReference>
            </connector>
            <label>Authorized No Final Result</label>
        </rules>
        <rules>
            <name>AuthorizedWithFinalResult</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>getGroupMember</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>GetRisk.FinalRisk__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>CompleteAction</targetReference>
            </connector>
            <label>Authorized With Final Result</label>
        </rules>
        <rules>
            <name>UnauthorizedOwner</name>
