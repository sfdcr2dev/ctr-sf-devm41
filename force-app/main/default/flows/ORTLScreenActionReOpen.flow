            <conditions>
                <leftValueReference>GetAction.OwnerId</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <elementReference>$User.Id</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>GetRisk.FinalRisk__c</leftValueReference>
