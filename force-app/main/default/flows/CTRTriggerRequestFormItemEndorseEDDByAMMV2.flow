                <targetReference>getCEO</targetReference>
            </connector>
            <label>IsEndorse</label>
        </rules>
        <rules>
            <name>IsRejected</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.EndorseDueDiligenceResult__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Rejected</stringValue>
