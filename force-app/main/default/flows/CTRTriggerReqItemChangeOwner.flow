        <defaultConnectorLabel>None</defaultConnectorLabel>
        <rules>
            <name>CreditRating</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Approval_Step__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>Credit</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>checkCreditCondition</targetReference>
            </connector>
            <label>Credit Rating</label>
        </rules>
        <rules>
            <name>SelectCommittee</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Approval_Step__c</leftValueReference>
                <operator>EqualTo</operator>
