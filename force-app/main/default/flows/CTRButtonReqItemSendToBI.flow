        <expression>IF(CONTAINS({!getRequestItem.RecordType.DeveloperName}, &quot;Customer&quot;), &quot;C&quot;, &quot;&quot;)
&amp;
IF(TEXT({!getRequestItem.AccountGroup__c}) = &quot;TRDL&quot;, &quot;1&quot;,
IF(TEXT({!getRequestItem.AccountGroup__c}) = &quot;TRDF&quot;, &quot;2&quot;,
IF(TEXT({!getRequestItem.AccountGroup__c}) = &quot;TOPG&quot;, &quot;3&quot;, &quot;&quot;)))
&amp;
IF({!getRequestItem.CTRRequestFormHeader__r.Country__r.Code__c} = &quot;ID&quot;, &quot;ID&quot;,
IF({!getRequestItem.CTRRequestFormHeader__r.Country__r.Code__c} = &quot;SG&quot;, &quot;SG&quot;,
IF({!getRequestItem.CTRRequestFormHeader__r.Country__r.Code__c} = &quot;IN&quot;, &quot;IN&quot;, &quot;&quot;)))
&amp;
LPAD(TEXT({!customerRunning}), 5, &quot;0&quot;)</expression>
    </formulas>
