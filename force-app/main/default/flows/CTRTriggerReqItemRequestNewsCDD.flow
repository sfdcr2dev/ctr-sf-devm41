    <description>Improved flow by using sub-flow to query recipient information and check recipient != null</description>
    <formulas>
        <name>emailCDDSubject</name>
        <dataType>String</dataType>
        <expression>&apos;[FYA] &apos; + {!fmlRecordType} + &apos;: CDD Results for &apos; + {!$Record.CompanyName__c} + &apos; are Ready to Review&apos;</expression>
    </formulas>
    <formulas>
        <name>emailSubject</name>
        <dataType>String</dataType>
        <expression>&apos;[FYA] &apos; + {!fmlRecordType} + &apos;: NEWS for &apos; + {!$Record.CompanyName__c} + &apos; is Ready to Review&apos;</expression>
    </formulas>
    <formulas>
        <name>fmlOrg</name>
        <dataType>String</dataType>
        <expression>IF(contains  ({!$Record.RecordType.DeveloperName}, &apos;Customer&apos;), TEXT({!$Record.SalesOrganization__c}), IF(contains({!$Record.RecordType.DeveloperName}, &apos;Supplier&apos;),TEXT({!$Record.PurchasingOrganization__c}),&apos;&apos;))</expression>
    </formulas>
    <formulas>
        <name>fmlRecLink</name>
        <dataType>String</dataType>
        <expression>LEFT({!$Api.Partner_Server_URL_260}, FIND( &apos;/services&apos;, {!$Api.Partner_Server_URL_260})) &amp; {!varTargetRec}</expression>
    </formulas>
    <formulas>
        <name>fmlRecordType</name>
        <dataType>String</dataType>
        <expression>CASE({!$Record.CTRRequestFormHeader__r.RecordType.DeveloperName},
&quot;NewCounterparty&quot;, &quot;Initial&quot;,
&quot;Extension&quot;, &quot;Extend&quot;,
&quot;EditInformation&quot;, &quot;Edit&quot;,
&quot;ShipToCreate&quot;, &quot;New Ship-to&quot;,
&quot;ShipToEdit&quot;, &quot;Edit Ship-to&quot;,
&quot;ShipToChckCountry&quot;, &quot;Check Ship-to&quot;,
&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>fmlType</name>
        <dataType>String</dataType>
        <expression>IF({!$Record.CTRRequestFormHeader__r.Country__r.Code__c} = &quot;TH&quot;, &quot;Domestic&quot;, &quot;International&quot;)</expression>
    </formulas>
    <formulas>
        <name>notifyBody</name>
        <dataType>String</dataType>
        <expression>&apos;Click to review news for this request.&apos;</expression>
    </formulas>
    <formulas>
        <name>notifyCDDBody</name>
        <dataType>String</dataType>
        <expression>&apos;Click to review CDD results for this request.&apos;</expression>
    </formulas>
    <formulas>
        <name>notifyCDDTitle</name>
        <dataType>String</dataType>
        <expression>&apos;CDD Results for &apos; + {!$Record.CompanyName__c} + &apos; are Ready for Review&apos;</expression>
    </formulas>
    <formulas>
        <name>notifyTitle</name>
        <dataType>String</dataType>
        <expression>&apos;News for &apos; + {!$Record.CompanyName__c} + &apos; is Ready for Review&apos;</expression>
    </formulas>
