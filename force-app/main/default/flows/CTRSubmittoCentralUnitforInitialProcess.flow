    <formulas>
        <name>fmlCountry</name>
        <dataType>String</dataType>
        <expression>IF({!GetRequestItemRecord.CTRRequestFormHeader__r.Country__r.Code__c} = &quot;TH&quot;, &quot;Domestic&quot;, &quot;International&quot;)</expression>
    </formulas>
    <formulas>
        <name>fmlNotifybody</name>
        <dataType>String</dataType>
        <expression>{!GetRequestItemRecord.Customer__r.Name} +&quot; &quot;+ &quot;need your support in verifying compliance. Please see the details.&quot;</expression>
    </formulas>
    <formulas>
        <name>fmlNotifySubject</name>
        <dataType>String</dataType>
        <expression>&quot;Request Number&quot;+&quot; &quot;+{!GetRequestItemRecord.Name}+&quot; &quot;+&quot;  New &quot;+ {!GetRequestItemRecord.CounterpartyType__c} +&quot; Need Compliance Check&quot;</expression>
    </formulas>
    <formulas>
        <name>fmlOrg</name>
        <dataType>String</dataType>
        <expression>IF(contains  ({!GetRequestItemRecord.RecordType.DeveloperName}, &apos;Customer&apos;), TEXT({!GetRequestItemRecord.SalesOrganization__c}), IF(contains({!GetRequestItemRecord.RecordType.DeveloperName}, &apos;Supplier&apos;),TEXT({!GetRequestItemRecord.PurchasingOrganization__c}),&apos;&apos;))</expression>
    </formulas>
