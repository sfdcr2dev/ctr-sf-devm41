						<lightning:inputField aura:id="Priority__c" fieldName="Priority__c" value="{!v.form.Priority__c}" />
						<lightning:inputField aura:id="High_Risk_Work__c" fieldName="High_Risk_Work__c" value="{!v.form.High_Risk_Work__c}" />
						<lightning:inputField aura:id="Latitude__c" fieldName="Latitude__c" value="{!v.form.Latitude__c}" />
						<lightning:inputField aura:id="Longitude__c" fieldName="Longitude__c" value="{!v.form.Longitude__c}" />
					</div>

					<!-- Notificition -->
					<lightning:layout horizontalAlign="spread">
						<lightning:layoutItem size="12" smallDeviceSize="6" largeDeviceSize="6" flexibility="auto" padding="horizontal-medium">
							<div class="slds-form-element slds-form-element_stacked">
								<label class="slds-form-element__label">Notificition</label>
								<div class="slds-form-element__control">
									<lightning:inputField
										aura:id="Notification__c"
										fieldName="Notification__c"
										value="{!v.form.Notification__c}"
										onchange="{!c.handleNotificationChanged}"
										variant="label-hidden"
									/>
								</div>
							</div>
						</lightning:layoutItem>

						<lightning:layoutItem size="12" smallDeviceSize="6" largeDeviceSize="6" flexibility="auto" padding="horizontal-medium">
							<lightning:select label="Priority" value="{!v.form.Priority__c}" disabled="true">
								<option value="">--None--</option>
								<option value="1" selected="{!v.form.Priority__c=='1'}">Immediately</option>
								<option value="2" selected="{!v.form.Priority__c=='2'}">Urgent</option>
								<option value="3" selected="{!v.form.Priority__c=='3'}">Normal</option>
								<option value="3" selected="{!v.form.Priority__c=='A'}">Float Time - 1M</option>
								<option value="3" selected="{!v.form.Priority__c=='C'}">Float Time - 3M</option>
								<option value="3" selected="{!v.form.Priority__c=='F'}">Float Time - 6M</option>
								<option value="3" selected="{!v.form.Priority__c=='I'}">Float Time - 9M</option>
								<option value="3" selected="{!v.form.Priority__c=='L'}">Float Time - 12M</option>
								<option value="3" selected="{!v.form.Priority__c=='O'}">Float Time - 15M</option>
								<option value="3" selected="{!v.form.Priority__c=='R'}">Float Time - 18M</option>
							</lightning:select>
						</lightning:layoutItem>
					</lightning:layout>

					<!-- Functional Location -->
