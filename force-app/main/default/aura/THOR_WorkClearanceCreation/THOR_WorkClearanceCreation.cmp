					<!-- Priority -->
					<div class="slds-hide">
						<lightning:inputField aura:id="isIRPA__c" fieldName="isIRPA__c" value="{!true}" disabled="true" />
						<lightning:inputField aura:id="Last_User_Action__c" fieldName="Last_User_Action__c" value="CRTE" disabled="true" />
						<lightning:inputField aura:id="User_Status__c" fieldName="User_Status__c" value="" disabled="true" />
						<lightning:inputField aura:id="Planning_Plant__c" fieldName="Planning_Plant__c" value="{!v.form.Planning_Plant__c}" disabled="true" />
						<lightning:inputField
							aura:id="Main_Work_Center__c"
							fieldName="Main_Work_Center__c"
							value="{!v.form.Main_Work_Center__c}"
							disabled="true"
						/>
