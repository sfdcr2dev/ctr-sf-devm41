							<label class="slds-form-element__label" for="Area"> Area (พื้นที่ปฎิบัติงาน)</label>
							<lightning:inputField variant="label-hidden" aura:id="Area_Code__c" fieldName="Area_Code__c" value="{!v.form.Area_Code__c}" disabled="true" class="slds-hide" />
							<lightning:inputField variant="label-hidden" aura:id="Area__c" fieldName="Area__c" value="{!v.form.Area__c}" disabled="true" />
							<lightning:inputField variant="label-hidden" aura:id="Maintenance_Plant__c" fieldName="Maintenance_Plant__c" value="{!v.form.Maintenance_Plant__c}" disabled="true" class="slds-hide" />
						</lightning:layoutItem>

