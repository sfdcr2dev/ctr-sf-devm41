<aura:component Implements="flexipage:availableForRecordHome,force:hasRecordId" controller="THOR_WorkClearanceController">
	<lightning:navigation aura:id="navService" />

	<aura:handler name="init" value="{!this}" action="{!c.doInit}" />

	<!-- <aura:attribute name="form" type="Work_Clearance__c" default="{ 'sobjectType':'Work_Clearance__c' }"/> -->

	<!-- <aura:attribute name="createNew" type="Boolean" default="false" access="global" /> -->
	<!-- <aura:attribute name="fromWC" type="Boolean" default="false" access="global" /> -->

	<aura:attribute name="recordId" type="String" default="" />
	<aura:attribute name="isShowModal" type="Boolean" default="false" />
	<!-- <aura:attribute name="notificationId" type="String" default=""/> -->
	<aura:attribute name="activeSectionsMessage" type="String" default="" />
	<aura:attribute name="activeSections" type="List" default="[]" />

	<aura:attribute name="currentPISUser" type="Map" default="{}" />

	<aura:attribute name="areas" type="List" default="[]" access="private" />
	<aura:attribute name="form" type="Map" default="{}" access="private" />
	<aura:attribute name="formOptionDisplay" type="Map" default="" access="private" />
	<aura:attribute name="formOption" type="Map" default="{}" access="private" />
	<aura:attribute name="objectInfoFields" type="Map" default="{}" access="private" />
	<aura:attribute name="isToggleSubModal" type="Map" default="{}" access="private" />
	<aura:attribute name="isLoading" type="Boolean" default="false" access="private" />
	<aura:attribute name="timeout" type="Integer" access="private" />

	<!-- show/hide -->
	<aura:attribute name="displayedSection" type="string" default="" />
	<aura:attribute name="isStep1" type="Boolean" default="false" access="private" />
	<aura:attribute name="isStep2" type="Boolean" default="false" access="private" />
	<aura:attribute name="isStep3" type="Boolean" default="false" access="private" />

	<aura:registerEvent name="closeModalEvent" type="c:THOR_Event" />
	<aura:handler name="closeModalEvent" event="c:THOR_Event" action="{!c.closeConfirmation}" />

	<aura:if isTrue="{!!v.isShowModal}">
		<div class="container">
			<lightning:button variant="brand" label="Create Work Clearance" title="Create Work Clearance" onclick="{!c.toggleModal}" />
		</div>
	</aura:if>

	<aura:if isTrue="{!v.isShowModal}">
