                            <lightning:icon iconName="utility:switch" alternativeText="switch" title="" size="x-small" class="slds-section__title-action-icon slds-button__icon slds-button__icon_left" />
                            <span class="slds-truncate">{!v.sectionLabelMap.cmReview.section}</span>
                        </button>
                    </h3>
                    <div class="slds-section__content" id="cmReviewDetail">
                        <div class="slds-grid slds-wrap slds-var-p-around_large">
                            <div class="slds-col slds-size_1-of-2 slds-var-p-bottom_medium slds-var-p-right_medium">
                                {!v.sectionLabelMap.cmReview.reviewInfo.EnhanceDueDiligenceProceedByTrader__c}:
                            </div>
                            <div class="slds-col slds-size_1-of-2 slds-var-p-bottom_medium slds-var-p-left_medium">
                                <!-- <lightning:radioGroup type="radio" aura:id="inputRadio" name="cmReviewResult" variant="label-hidden" options="{!v.reviewOptions}" value="{!v.cmReviewResult}" disabled="{!!v.isCMReview}" class="display-flex" required="{!v.isCMReview}" /> -->
                                <lightning:inputField aura:id="inputField" variant="label-hidden" fieldName="EnhanceDueDiligenceProceedByTrader__c" disabled="{!!v.isCMReview}" required="{!v.isCMReview}" />
                            </div>
                            <div class="slds-col slds-size_1-of-1 slds-var-p-bottom_medium">
                                <div class="slds-form">
                                    <div class="slds-form-element">
                                        <label class="slds-form-element__label" for="horizontal-input-id-02">
                                            {!v.sectionLabelMap.cmReview.reviewInfo.EnhanceDueDiligenceCommentByTrader__c}:
                                        </label>
                                        <div class="slds-form-element__control">
                                            <lightning:inputField aura:id="inputField" variant="label-hidden" fieldName="EnhanceDueDiligenceCommentByTrader__c" disabled="{!!v.isCMReview}" required="{!v.isCMReview}" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="slds-col slds-size_1-of-2 slds-var-p-bottom_medium slds-var-p-right_medium">
                                <div class="slds-form">
                                    <div class="slds-form-element">
                                        <label class="slds-form-element__label" for="horizontal-input-id-02">
                                            {!v.sectionLabelMap.cmReview.reviewInfo.TraderEnhanceDueDiligenceBy__c}:
                                        </label>
                                        <div class="slds-form-element__control">
                                            <lightning:inputField aura:id="inputField" variant="label-hidden" fieldName="TraderEnhanceDueDiligenceBy__c" disabled="true" class="lookup-disabled" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="slds-col slds-size_1-of-2 slds-var-p-bottom_medium slds-var-p-left_medium">
                                <div class="slds-form">
                                    <div class="slds-form-element">
                                        <label class="slds-form-element__label" for="horizontal-input-id-02">
                                            {!v.sectionLabelMap.cmReview.reviewInfo.TraderEnhanceDueDiligenceDateTime__c}:
                                        </label>
                                        <div class="slds-form-element__control">
                                            <lightning:inputField aura:id="inputField" variant="label-hidden" fieldName="TraderEnhanceDueDiligenceDateTime__c" disabled="true" class="lookup-disabled" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </aura:renderIf>

            <aura:renderIf isTrue="{!v.displayEnhanceDueDiligenceSection}">
                <div class="slds-section slds-is-open" aura:id="enhanceDueDiligenceSection">
                    <h3 class="slds-section__title">
                        <button type="button" aria-controls="enhanceDueDiligenceDetail" aria-expanded="true" class="slds-button slds-section__title-action" data-auraid="enhanceDueDiligenceSection" onclick="{!c.handleToggleSection}">
                            <lightning:icon iconName="utility:switch" alternativeText="switch" title="" size="x-small" class="slds-section__title-action-icon slds-button__icon slds-button__icon_left" />
                            <span class="slds-truncate">{!v.sectionLabelMap.enhanceDueDiligence.section}</span>
                        </button>
                    </h3>
                    <div class="slds-section__content" id="enhanceDueDiligenceDetail">
                        <div class="slds-grid slds-wrap slds-var-p-around_large">
                            <div class="slds-col slds-size_1-of-2 slds-var-p-bottom_medium slds-var-p-right_medium">
                                {!v.sectionLabelMap.enhanceDueDiligence.reviewInfo.EnhanceDueDiligenceResult__c}
                            </div>
                            <div class="slds-col slds-size_1-of-2 slds-var-p-bottom_medium slds-var-p-left_medium">
                                <lightning:inputField aura:id="inputField" variant="label-hidden" fieldName="EnhanceDueDiligenceResult__c" disabled="{!!v.isEnhanceDueDiligence}" required="{!v.isEnhanceDueDiligence}" />
                            </div>
                            <div class="slds-col slds-size_1-of-1 slds-var-p-bottom_medium">
                                <div class="slds-form">
                                    <div class="slds-form-element">
                                        <label class="slds-form-element__label" for="horizontal-input-id-02">
                                            {!v.sectionLabelMap.enhanceDueDiligence.reviewInfo.EnhanceDueDiligenceCommentByEDD__c}:
                                        </label>
                                        <div class="slds-form-element__control">
                                            <lightning:inputField aura:id="inputField" variant="label-hidden" fieldName="EnhanceDueDiligenceCommentByEDD__c" disabled="{!!v.isEnhanceDueDiligence}" required="{!v.isEnhanceDueDiligence}" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="slds-col slds-size_1-of-2 slds-var-p-bottom_medium slds-var-p-right_medium">
                                <div class="slds-form">
                                    <div class="slds-form-element">
                                        <label class="slds-form-element__label" for="horizontal-input-id-02">
                                            {!v.sectionLabelMap.enhanceDueDiligence.reviewInfo.EnhanceDueDiligenceBy__c}:
                                        </label>
                                        <div class="slds-form-element__control">
                                            <lightning:inputField aura:id="inputField" variant="label-hidden" fieldName="EnhanceDueDiligenceBy__c" disabled="true" class="lookup-disabled" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="slds-col slds-size_1-of-2 slds-var-p-bottom_medium slds-var-p-left_medium">
                                <div class="slds-form">
                                    <div class="slds-form-element">
                                        <label class="slds-form-element__label" for="horizontal-input-id-02">
                                            {!v.sectionLabelMap.enhanceDueDiligence.reviewInfo.EnhanceDueDiligenceDateTime__c}:
                                        </label>
                                        <div class="slds-form-element__control">
                                            <lightning:inputField aura:id="inputField" variant="label-hidden" fieldName="EnhanceDueDiligenceDateTime__c" disabled="true" class="lookup-disabled" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </aura:renderIf>

            <aura:renderIf isTrue="{!v.displayComplianceAdviceSection}">
