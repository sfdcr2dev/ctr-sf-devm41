                </aura:renderIf>
            </div>
        </aura:renderIf>
        <div class="slds-section slds-is-open" aura:id="dueDiligenceResultSection">
            <h3 class="slds-section__title">
                <button type="button" aria-controls="dueDiligenceResultDetail" aria-expanded="true" class="slds-button slds-section__title-action" data-auraid="dueDiligenceResultSection" onclick="{!c.handleToggleSection}">
                    <lightning:icon iconName="utility:switch" alternativeText="switch" title="" size="x-small" class="slds-section__title-action-icon slds-button__icon slds-button__icon_left" />
                    <span class="slds-truncate">Due Diligence Result</span>
                </button>
            </h3>
            <div class="slds-section__content" id="dueDiligenceResultDetail">
                <c:CTRDueDiligenceResult recordId="{!v.recordId}" sObjectName="{!v.sObjectName}" displayRiskBySystem="false" />
            </div>
        </div>
        
        <lightning:recordEditForm aura:id="recordEditForm" recordId="{!v.recordId}" objectApiName="{!v.sObjectName}" onload="{!c.handleLoad}" onsubmit="{!c.handleSubmit}" onsuccess="{!c.handleSuccess}" onerror="{!c.handleError}">
            <aura:renderIf isTrue="{!v.displayCMReviewSection}">
                <div class="slds-section slds-is-open" aura:id="cmReviewSection">
                    <h3 class="slds-section__title">
