<aura:component implements="flexipage:availableForAllPageTypes,force:hasRecordId,force:hasSObjectName,force:lightningQuickActionWithoutHeader" controller="CTRCheckDueDiligenceController" access="global">
    <aura:attribute name="recordId" type="String" />
    <aura:attribute name="sObjectName" type="String" />
    <aura:attribute name="reviewOptions" type="List" default="[ {'label': 'Proceed', 'value': 'Proceed'}, {'label': 'Not Proceed', 'value': 'Not Proceed'}]" />
    <!-- <aura:attribute name="cmReviewOptions" type="List" default="[ {'label': 'Proceed', 'value': 'Proceed'}, {'label': 'Not Proceed', 'value': 'Not Proceed'}]" /> -->
    <aura:attribute name="complianceAdviceOptions" type="List" default="[ {'label': 'Yes', 'value': 'Yes'}, {'label': 'No', 'value': 'No'}]" />
    <!-- <aura:attribute name="cpxxReviewOptions" type="List" default="[ {'label': 'Proceed', 'value': 'Proceed'}, {'label': 'Not Proceed', 'value': 'Not Proceed'}]" /> -->
    <!-- <aura:attribute name="cmvpReviewOptions" type="List" default="[ {'label': 'Proceed', 'value': 'Proceed'}, {'label': 'Not Proceed', 'value': 'Not Proceed'}]" /> -->
    <aura:attribute name="endorseOptions" type="List" default="[ {'label': 'Endorsed', 'value': 'Endorsed'}, {'label': 'Rejected', 'value': 'Rejected'}]" />
    <aura:attribute name="approveOptions" type="List" default="[ {'label': 'Approved', 'value': 'Approved'}, {'label': 'Rejected', 'value': 'Rejected'}]" />
    <aura:attribute name="sectionLabelMap" type="Object" />

    <aura:attribute name="isCMReview" type="Boolean" default="false" />
    <aura:attribute name="isEnhanceDueDiligence" type="Boolean" default="false" />
    <aura:attribute name="isComplianceAdvice" type="Boolean" default="false" />
    <aura:attribute name="isCPXXReview" type="Boolean" default="false" />
    <aura:attribute name="isCMVPReview" type="Boolean" default="false" />
    <aura:attribute name="isEVPCReview" type="Boolean" default="false" />
    <aura:attribute name="isEndorseDueDiligence" type="Boolean" default="false" />
    <aura:attribute name="isApproveDueDiligence" type="Boolean" default="false" />
    <aura:attribute name="isEdit" type="Boolean" default="false" />
    <aura:attribute name="currentSection" type="String" />

    <aura:attribute name="displayCMReview" type="Boolean" default="false" />
    <aura:attribute name="displayEnhanceDueDiligence" type="Boolean" default="false" />
    <aura:attribute name="displayComplianceAdvice" type="Boolean" default="false" />
    <aura:attribute name="displayCPXXReview" type="Boolean" default="false" />
    <aura:attribute name="displayCMVPReview" type="Boolean" default="false" />
    <aura:attribute name="displayEVPCReview" type="Boolean" default="false" />
    <aura:attribute name="displayEndorseDueDiligence" type="Boolean" default="false" />
    <aura:attribute name="displayApproveDueDiligence" type="Boolean" default="false" />

    <aura:attribute name="displayCMReviewButton" type="Boolean" default="false" />
    <aura:attribute name="displayEnhanceDueDiligenceButton" type="Boolean" default="false" />
    <aura:attribute name="displayComplianceAdviceButton" type="Boolean" default="false" />
    <aura:attribute name="displayCPXXReviewButton" type="Boolean" default="false" />
    <aura:attribute name="displayCMVPReviewButton" type="Boolean" default="false" />
    <aura:attribute name="displayEVPCReviewButton" type="Boolean" default="false" />
    <aura:attribute name="displayEndorseDueDiligenceButton" type="Boolean" default="false" />
    <aura:attribute name="displayApproveDueDiligenceButton" type="Boolean" default="false" />

    <aura:attribute name="displayCMReviewSection" type="Boolean" default="false" />
    <aura:attribute name="displayEnhanceDueDiligenceSection" type="Boolean" default="false" />
    <aura:attribute name="displayComplianceAdviceSection" type="Boolean" default="false" />
    <aura:attribute name="displayCPXXReviewSection" type="Boolean" default="false" />
    <aura:attribute name="displayCMVPReviewSection" type="Boolean" default="false" />
    <aura:attribute name="displayEVPCReviewSection" type="Boolean" default="false" />
    <aura:attribute name="displayEndorseDueDiligenceSection" type="Boolean" default="false" />
    <aura:attribute name="displayApproveDueDiligenceSection" type="Boolean" default="false" />

    <aura:attribute name="cmReviewResult" type="String" />
    <aura:attribute name="complianceAdviceResult" type="String" />
    <aura:attribute name="cpxxReviewResult" type="String" />
    <aura:attribute name="cmvpReviewResult" type="String" />
    <aura:attribute name="evpcReviewResult" type="String" />
    <aura:attribute name="endorseDueDiligenceResult" type="String" />
    <aura:attribute name="approveDueDiligenceResult" type="String" />
    <aura:attribute name="recordObject" type="Object" />
    <aura:attribute name="recordLoadError" type="String" />
    <aura:attribute name="isHighHighRisk" type="Boolean" default="false" />
    <aura:attribute name="isRejected" type="Boolean" default="false" />
    <aura:attribute name="isSupplier" type="Boolean" default="false" />
    <aura:attribute name="isTX" type="Boolean" default="false" />
    <aura:attribute name="isLoaded" type="Boolean" default="false" />

    <force:recordData aura:id="recordLoader" recordId="{!v.recordId}" fields="Id,Name,Status__c,Approval_Step__c,EnhanceDueDiligenceProceedByTrader__c,TraderEnhanceDueDiligenceBy__c,EnhanceDueDiligenceBy__c,ComplianceAdviceResult__c,ComplianceAdviceBy__c,
                                                                              CPXXReviewDueDiligenceResult__c,CPXXReviewDueDiligenceBy__c,CMVPReviewDueDiligenceResult__c,CMVPReviewDueDiligenceBy__c,EVPCReviewEDDResult__c,EVPCReviewEDDBy__c,
                                                                              EndorseDueDiligenceResult__c,EndorseDueDiligenceBy__c,ApproveDueDiligenceResult__c,ApproveDueDiligenceBy__c,FinalRiskLevel__c,RecordType.DeveloperName" targetFields="{!v.recordObject}" targetError="{!v.recordLoadError}" recordUpdated="{!c.handleRecordUpdated}" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />

    <section>
        <aura:renderIf isTrue="{!and(!v.isEdit, !v.isRejected)}">
            <div class="slds-text-align_right slds-var-p-bottom_xx-small">
                <aura:renderIf isTrue="{!v.displayCMReviewButton}">
                    <lightning:button variant="brand-outline" label="{!v.sectionLabelMap.cmReview.button}" title="{!v.sectionLabelMap.cmReview.button}" name="isCMReview" onclick="{!c.handleEdit}" />
                </aura:renderIf>
                <aura:renderIf isTrue="{!v.displayEnhanceDueDiligenceButton}">
                    <lightning:button variant="brand-outline" label="{!v.sectionLabelMap.enhanceDueDiligence.button}" title="{!v.sectionLabelMap.enhanceDueDiligence.button}" name="isEnhanceDueDiligence" onclick="{!c.handleEdit}" />
                </aura:renderIf>
                <aura:renderIf isTrue="{!v.displayComplianceAdviceButton}">
                    <lightning:button variant="brand-outline" label="{!v.sectionLabelMap.complianceAdvice.button}" title="{!v.sectionLabelMap.complianceAdvice.button}" name="isComplianceAdvice" onclick="{!c.handleEdit}" />
                </aura:renderIf>
                <aura:renderIf isTrue="{!v.displayCPXXReviewButton}">
                    <lightning:button variant="brand-outline" label="{!v.sectionLabelMap.cpxxReview.button}" title="{!v.sectionLabelMap.cpxxReview.button}" name="isCPXXReview" onclick="{!c.handleEdit}" />
                </aura:renderIf>
                <aura:renderIf isTrue="{!v.displayCMVPReviewButton}">
                    <lightning:button variant="brand-outline" label="{!v.sectionLabelMap.cmvpReview.button}" title="{!v.sectionLabelMap.cmvpReview.button}" name="isCMVPReview" onclick="{!c.handleEdit}" />
