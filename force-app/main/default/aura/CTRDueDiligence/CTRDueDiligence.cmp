                </aura:renderIf>
                <aura:renderIf isTrue="{!v.displayEndorseDueDiligenceButton}">
                    <lightning:button variant="brand-outline" label="{!v.sectionLabelMap.endorseDueDiligence.button}" title="{!v.sectionLabelMap.endorseDueDiligence.button}" name="isEndorseDueDiligence" onclick="{!c.handleEdit}" />
                </aura:renderIf>
                <aura:renderIf isTrue="{!v.displayApproveDueDiligenceButton}">
