                        <div class="slds-grid slds-wrap slds-var-p-around_large">
                            <div class="slds-col slds-size_1-of-2 slds-var-p-bottom_medium slds-var-p-right_medium">
                                {!v.sectionLabelMap.complianceAdvice.reviewInfo.ComplianceAdviceResult__c}
                            </div>
                            <div class="slds-col slds-size_1-of-2 slds-var-p-bottom_medium slds-var-p-left_medium">
                                <!-- <lightning:radioGroup type="radio" aura:id="inputRadio" name="complianceAdviceResult" variant="label-hidden" options="{!v.complianceAdviceOptions}" value="{!v.complianceAdviceResult}" disabled="{!!v.isComplianceAdvice}" class="display-flex" required="{!v.isComplianceAdvice}" /> -->
                                <lightning:inputField aura:id="inputField" variant="label-hidden" fieldName="ComplianceAdviceResult__c" disabled="{!!v.isComplianceAdvice}" required="{!v.isComplianceAdvice}" />
                            </div>
                            <div class="slds-col slds-size_1-of-1 slds-var-p-bottom_medium">
                                <div class="slds-form">
                                    <div class="slds-form-element">
                                        <label class="slds-form-element__label" for="horizontal-input-id-02">
                                            {!v.sectionLabelMap.complianceAdvice.reviewInfo.ComplianceAdviceComment__c}:
                                        </label>
                                        <div class="slds-form-element__control">
                                            <lightning:inputField aura:id="inputField" variant="label-hidden" fieldName="ComplianceAdviceComment__c" disabled="{!!v.isComplianceAdvice}" required="{!v.isComplianceAdvice}" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="slds-col slds-size_1-of-2 slds-var-p-bottom_medium slds-var-p-right_medium">
                                <div class="slds-form">
                                    <div class="slds-form-element">
                                        <label class="slds-form-element__label" for="horizontal-input-id-02">
                                            {!v.sectionLabelMap.complianceAdvice.reviewInfo.ComplianceAdviceBy__c}:
                                        </label>
                                        <div class="slds-form-element__control">
                                            <lightning:inputField aura:id="inputField" variant="label-hidden" fieldName="ComplianceAdviceBy__c" disabled="true" class="lookup-disabled" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="slds-col slds-size_1-of-2 slds-var-p-bottom_medium slds-var-p-left_medium">
                                <div class="slds-form">
                                    <div class="slds-form-element">
                                        <label class="slds-form-element__label" for="horizontal-input-id-02">
                                            {!v.sectionLabelMap.complianceAdvice.reviewInfo.ComplianceAdviceDateTime__c}:
                                        </label>
                                        <div class="slds-form-element__control">
                                            <lightning:inputField aura:id="inputField" variant="label-hidden" fieldName="ComplianceAdviceDateTime__c" disabled="true" class="lookup-disabled" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </aura:renderIf>

            <aura:renderIf isTrue="{!v.displayCPXXReviewSection}">
                <div class="slds-section slds-is-open" aura:id="cpxxReviewSection">
                    <h3 class="slds-section__title">
                        <button type="button" aria-controls="cpxxReviewDetail" aria-expanded="true" class="slds-button slds-section__title-action" data-auraid="cpxxReviewSection" onclick="{!c.handleToggleSection}">
                            <lightning:icon iconName="utility:switch" alternativeText="switch" title="" size="x-small" class="slds-section__title-action-icon slds-button__icon slds-button__icon_left" />
                            <span class="slds-truncate">{!v.sectionLabelMap.cpxxReview.section}</span>
                        </button>
                    </h3>
                    <div class="slds-section__content" id="cpxxReviewDetail">
                        <div class="slds-grid slds-wrap slds-var-p-around_large">
                            <!-- <div class="slds-col slds-size_1-of-2 slds-var-p-bottom_medium">
                                {!v.sectionLabelMap.cpxxReview.reviewInfo.CPXXReviewDueDiligenceResult__c}:
                            </div>
                            <div class="slds-col slds-size_1-of-2 slds-var-p-bottom_medium">
                                <lightning:radioGroup type="radio" aura:id="inputRadio" name="cpxxReviewResult" variant="label-hidden" options="{!v.reviewOptions}" value="{!v.cpxxReviewResult}" disabled="{!!v.isCPXXReview}" class="display-flex" required="{!v.isCPXXReview}" />
                            </div> -->
