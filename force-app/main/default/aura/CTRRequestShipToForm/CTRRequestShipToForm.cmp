                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_1-of-2 slds-p-bottom_x-small">
                                            <lightning:recordEditForm recordId="{!v.recordId}" objectApiName="CTRRequestFormItem__c">
                                                <div class="slds-form-element">
                                                    <label class="slds-form-element__label" for="Account Assignment Group">
                                                        <abbr class="slds-required" title="required">*</abbr>Account
                                                        Assignment
                                                        Group</label>
                                                    <lightning:inputField aura:id="editFormAccountField"
                                                        variant="label-hidden" id="Account Assignment Group"
                                                        fieldName="AccountAssignmentGroup__c"
                                                        name="AccountAssignmentGroup__c" required="true"
                                                        onchange="{!c.handleAddJsonItem}" />
                                                    <div aura:id="error-AccountAssignmentGroup__c"
                                                        class="slds-form-element__help custom-error">
                                                        Complete this field.</div>
                                                </div>
                                            </lightning:recordEditForm>
                                        </div>

                                        <!-- Tax classification -->
                                        <div
                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_1-of-2 slds-p-bottom_x-small">
                                            <lightning:recordEditForm recordId="{!v.defaultInputRecordId}" objectApiName="{!v.useSObjectName}">
                                                <div class="slds-form-element">
                                                    <label class="slds-form-element__label" for="Tax classification">
                                                        Tax
                                                        classification</label>
                                                    <lightning:inputField aura:id="editFormAccountField"
                                                        variant="label-hidden" id="Tax classification"
                                                        fieldName="{!(v.sObjectName == 'Account' ? 'TaxClassification__c' : 'TAXClassification__c')}" name="TAXClassification__c"
                                                        disabled="true" />
                                                    <div aura:id="error-TAXClassification__c"
                                                        class="slds-form-element__help custom-error">
                                                        Complete this field.</div>
                                                </div>
                                            </lightning:recordEditForm>
                                        </div>
                                    </div>
                                </div>
                            </div>


                    </lightning:layoutItem>
                </div>

                <footer class="slds-modal__footer slds-text-align_center">
                    <lightning:button type="cancel" variant="brand-outline" label="Cancel" title="Cancel"
                        onclick="{!c.closeModal}" />
                    <lightning:button type="submit" variant="brand" label="Save" title="Save"
                        onclick="{!c.handleSubmit}" />
                </footer>
            </div>
        </section>
    </aura:if>
    <!-- </lightning:recordEditForm> -->
</aura:component>