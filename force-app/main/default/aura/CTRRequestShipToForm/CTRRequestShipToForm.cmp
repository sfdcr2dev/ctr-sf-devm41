<aura:component controller="CTRRequestShipToController"
    implements="lightning:hasPageReference,lightning:actionOverride,force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,forceCommunity:availableForAllPageTypes,force:lightningQuickAction"
    access="global">
    <!-- SENT FROM CTRCreateRequestLandingPage -->
    <aura:attribute name="customerId" type="Id" />
    <aura:attribute name="recordTypeId" type="Id" />
    <aura:attribute name="bu" type="String" />
    <aura:attribute name="requestType" type="String" />
    <!--  -->

    <!-- SENT FROM CTRRequestFormItemEdit -->
    <!-- <aura:attribute name="recordId" type="Id" /> -->
    <aura:attribute name="sObjectName" type="String" default="Account" />
    <aura:attribute name="useSObjectName" type="String" default="Account" />
    <!-- recordId: component.get('v.recordId'),
    sObjectName: component.get('v.sObjectName'),
    recordHeader: recordHeader,
    recordObject: component.get('v.recordObject'),
    SalesOrg: DefaultSalesOrg,
    PurchasingOrg: DefaultPurchasingOrg,
    isOtherProducts: isOtherProducts,
    bu: businessUnit,
    currentUserName: component.get('v.currentUserName'),
    Wth: wth,
    requestType: recordTypeName -->
    <!--  -->

    <aura:attribute name="showSpinner" type="Boolean" default="true" />
    <aura:attribute name="recordOwner" type="String" />
    <aura:attribute name="targetRecord" type="Object" />
    <aura:attribute name="itemRecord" type="Object" />
    <aura:attribute name="selectedRequest" type="Map" />
    <aura:attribute name="recordId" type="Id" /> <!-- item record id -->
    <aura:attribute name="formAccountLoaded" type="Boolean" default="false" />
    <aura:attribute name="recordHeader" type="Object" />
    <aura:attribute name="recordHeaderId" type="Id" />
    <aura:attribute name="useCustomerId" type="Id" />
    <aura:attribute name="defaultInputRecordId" type="Id" />
    <aura:attribute name="isEdit" type="Boolean" default="false" />
    <aura:attribute name="isInitial" type="Boolean" default="false" />
    <aura:attribute name="ChangedFieldForEDITHeader" type="List" default="[]" />
    <aura:attribute name="ChangedFieldForEDITItem" type="List" default="[]" />
    <aura:attribute name="currentUserName" type="String" />

    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />

    <aura:html tag="style">
        .slds-modal__container {
        width: 90% !important;
        max-width: 80% !important;
        height: 100% !important;
        }
        .cuf-content {
        padding: 0 0rem !important;
        }
        .slds-p-around--medium {
        padding: 0rem !important;
        }
        .slds-modal__content {
        overflow-y: auto !important;
        height: unset !important;
        max-height: unset !important;
        }
        .closeIcon {
        display: none !important;
        }
    </aura:html>
    <aura:if isTrue="{!v.isInitial}">
        <force:recordData aura:id="accountRecord" recordId="{!v.useCustomerId}"
            fields="Name, OwnerId, Owner.Name, TaxClassification__c, CompanyNameENF__c, CompanyNameLocal__c, TaxCode1__c"
            targetFields="{!v.targetRecord}" targetError="{!v.targetRecordError}"
            recordUpdated="{!c.handleRecordUpdated}" />
    </aura:if>

    <!-- <aura:if isTrue="{!v.recordId}">
        <force:recordData aura:id="itemRecord" recordId="{!v.recordId}"
            fields="Id, Name, toLabel(SalesOrganizationTX__c)" targetFields="{!v.itemRecord}"
            recordUpdated="{!c.handleItemRecordUpdated}" />
    </aura:if> -->


    <aura:if isTrue="{!v.showSpinner}">
        <lightning:spinner alternativeText="Loading" size="medium" />
    </aura:if>
    <!-- <lightning:recordEditForm aura:id="recordEditForm" recordId="{!v.customerId}" objectApiName="Account"
        onsubmit="{!c.handleSubmit}" onsuccess="{!c.handleSuccess}" onerror="{!c.handleError}" onload="{!c.handleLoad}"> -->
    <aura:if isTrue="{!v.isInitial}">
        <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true"
            class="slds-modal slds-fade-in-open"> <!-- style="padding-top:10%; width:200%; margin-left:-50%;" -->
            <div class="slds-modal__container custom_modal_container">
                <!-- HEADER -->
                <div class="slds-modal__header">
                    <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">
                        <!-- {!v.requestType} -->
                        <aura:if isTrue="{!v.isEdit}">
                            Edit
                            <aura:set attribute="else">Create</aura:set>
                        </aura:if>
                        Ship-To
                    </h2>
                </div>
                <!-- BODY -->
                <div class="slds-modal__content slds-p-around_medium">
                    <lightning:layoutItem padding="around-small">
                        <aura:renderIf isTrue="{!and(v.isEdit)}">
                            <div class="slds-section slds-is-open animate" aura:id="SelectedSalesArea">
                                <!--section header-->
                                <h3 class="slds-section__title">
                                    <a aria-controls="SelectedSalesAreaList"
                                        class="slds-button slds-section__title-action" onclick="{!c.toggleSection}"
                                        data-auraId="SelectedSalesArea">
                                        <lightning:icon iconName="utility:switch" size="x-small"
                                            class="slds-section__title-action-icon slds-button__icon_left"
                                            alternativeText="button icon" />
                                        <span class="slds-truncate" title="Selected Sales Area">Selected Sales
                                            Area</span>
                                    </a>
                                </h3>
                                <!--section body-->
                                <div class="slds-section__content" id="ShipToInformationList">
                                    <div class="slds-grid slds-wrap slds-var-p-around_large">
                                        <!-- Seller -->
                                        <div
                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_2-of-4 slds-p-bottom_x-small">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label" for="Seller">
                                                    Seller</label>
                                                <lightning:input id="Seller" name="Seller"
                                                    value="{!v.selectedRequest.mLabelSalesOrg}" variant="label-hidden"
                                                    disabled="true" />
                                            </div>
                                        </div>

                                        <!-- Distribution Channel -->
                                        <div
                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_2-of-4 slds-p-bottom_x-small">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label" for="Distribution Channel">
                                                    Distribution Channel</label>
                                                <lightning:input id="Distribution Channel"
                                                    value="{!v.selectedRequest.mLabelDistributionChannel}"
                                                    variant="label-hidden" name="DistributionChannel" disabled="true" />
                                            </div>
                                        </div>

                                        <!-- Country -->
                                        <div
                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_2-of-4 slds-p-bottom_x-small">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label" for="Country">
                                                    Country</label>
                                                <lightning:input id="Country" value="{!v.selectedRequest.mCountry}"
                                                    variant="label-hidden" name="Country" disabled="true" />
                                            </div>
                                        </div>

                                        <!-- Division -->
                                        <div
                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_2-of-4 slds-p-bottom_x-small">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label" for="Division">
                                                    Division</label>
                                                <lightning:input id="Division"
                                                    value="{!v.selectedRequest.mLabelDivision}" variant="label-hidden"
                                                    name="Division" disabled="true" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </aura:renderIf>

                        <lightning:recordEditForm aura:id="recordEditForm" recordId="{!v.defaultInputRecordId}"
                            objectApiName="{!v.useSObjectName}" onsuccess="{!c.handleSuccess}"
                            onerror="{!c.handleError}" onload="{!c.handleLoad}">
                            <div class="slds-section slds-is-open animate" aura:id="ShipToInformation">
                                <!--section header-->
                                <h3 class="slds-section__title">
                                    <a aria-controls="ShipToInformationList"
                                        class="slds-button slds-section__title-action" onclick="{!c.toggleSection}"
                                        data-auraId="ShipToInformation">
                                        <lightning:icon iconName="utility:switch" size="x-small"
                                            class="slds-section__title-action-icon slds-button__icon_left"
                                            alternativeText="button icon" />
                                        <span class="slds-truncate" title="Ship-To Information">Ship-To
                                            Information</span>
                                    </a>
                                </h3>
                                <!--section body-->
                                <div class="slds-section__content" id="ShipToInformationList">
                                    <div class="slds-grid slds-wrap slds-var-p-around_large">
                                        <!-- CompanyName (Local) -->
                                        <div
                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_2-of-4 slds-p-bottom_x-small">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label" for="Company Name">
                                                    <abbr class="slds-required" title="required">*</abbr>Company
                                                    Name</label>
                                                <lightning:inputField id="Company Name" value="{!v.targetRecord.Name}"
                                                    fieldName="Name" name="Name" variant="label-hidden" required="true"
                                                    disabled="true" />
                                            </div>
                                        </div>

                                        <!-- Account Owner -->
                                        <div
                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_2-of-4 slds-p-bottom_x-small">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label" for="Owner.Name">
                                                    Account Owner</label>
                                                <p class="custom-output">{!v.recordOwner}</p>
                                            </div>
                                        </div>

                                        <!-- ShipToNameEng -->
                                        <div
                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_2-of-4 slds-p-bottom_x-small">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label" for="Ship-To Name (English)">
                                                    <abbr class="slds-required" title="required">*</abbr>Ship-To Name
                                                    (English)</label>
                                                <lightning:inputField aura:id="editFormAccountField"
                                                    id="Ship-To Name (English)" fieldName="CompanyNameENF__c"
                                                    onchange="{!c.handleAddJsonHeader}" variant="label-hidden"
                                                    name="CompanyNameENF__c" />
                                            </div>
                                        </div>

                                        <!-- ShipToNameOther -->
                                        <div
                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_2-of-4 slds-p-bottom_x-small">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label" for="Ship-To Name (Other Languages)">
                                                    Ship-To Name (Other Languages)
                                                    <lightning:helptext class="custom-help-txt"
                                                        content="This field will auto default as Ship-To Name (English). Users can change name as requires." />
                                                </label>
                                                <lightning:inputField aura:id="editFormAccountField"
                                                    id="Ship-To Name (Other Languages)" name="CompanyNameLocal__c"
                                                    fieldName="{!(v.sObjectName == 'Account' ? 'CompanyNameLocal__c' : 'CustomerNameLocal1__c')}"
                                                    onchange="{!c.handleAddJsonHeader}" variant="label-hidden" />
                                                <div aura:id="error-CompanyNameLocal__c"
                                                    class="slds-form-element__help custom-error">
                                                    Complete this field.</div>
                                            </div>
                                        </div>

                                        <aura:renderIf isTrue="{!!v.isEdit}">
                                            <!-- TAX Number -->
                                            <div
                                                class="slds-p-right_medium slds-p-left_medium slds-col slds-size_2-of-4 slds-p-bottom_x-small">
                                                <div class="slds-form-element">
                                                    <lightning:inputField aura:id="editFormAccountField"
                                                        id="TaxCode1__c" variant="brand" fieldName="TaxCode1__c"
                                                        name="TaxCode1__c" disabled="true"
                                                        value="{!v.targetRecord.TaxCode1__c}" />
                                                    <div aura:id="error-TaxCode1__c"
                                                        class="slds-form-element__help custom-error">Complete
                                                        this field.</div>
                                                </div>
                                            </div>
                                        </aura:renderIf>
                                    </div>
                                </div>
                            </div>

                            <div class="slds-section slds-is-open animate" aura:id="SearchTerms">
                                <!--section header-->
                                <h3 class="slds-section__title">
                                    <a aria-controls="SearchTermsList" class="slds-button slds-section__title-action"
                                        onclick="{!c.toggleSection}" data-auraId="SearchTerms">
                                        <lightning:icon iconName="utility:switch" size="x-small"
                                            class="slds-section__title-action-icon slds-button__icon_left"
                                            alternativeText="button icon" />
                                        <span class="slds-truncate" title="Search Terms">Search Terms</span>
                                    </a>
                                </h3>
                                <div class="slds-section__content" id="SearchTermsList">
                                    <div class="slds-grid slds-wrap slds-var-p-around_large">
                                        <!-- Search Team 1 -->
                                        <div
                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_2-of-4 slds-p-bottom_x-small">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label"
                                                    for="Search Term 1">
                                                    <abbr class="slds-required" title="required">*</abbr>Search Term
                                                    1</label>
                                                <lightning:inputField aura:id="editFormAccountField"
                                                    class="help-txt-hidden" variant="label-hidden"
                                                    id="Search Term 1"
                                                    fieldName="CustomerSearchTermLocal__c"
                                                    name="CustomerSearchTermLocal__c" required="true"
                                                    onchange="{!c.handleAddJsonHeader}" />
                                                <div aura:id="error-CustomerSearchTermLocal__c"
                                                    class="slds-form-element__help custom-error">Complete this field.
                                                </div>
                                            </div>
                                        </div>

                                        <!-- Search Team 2 (English) -->
                                        <div
                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_2-of-4 slds-p-bottom_x-small">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label" for="Search Team 2 (English)">
                                                    Search Team 2 (English)</label>
                                                <lightning:inputField aura:id="editFormAccountField"
                                                    variant="label-hidden" id="Search Team 2 (English)"
                                                    fieldName="CustomerSearchTermEN__c" name="CustomerSearchTermEN__c"
                                                    onchange="{!c.handleAddJsonHeader}" />
                                                <div aura:id="error-CustomerSearchTermEN__c"
                                                    class="slds-form-element__help custom-error">Complete this field.
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="slds-section slds-is-open animate" aura:id="RegisteredAddress">
                                <!--section header-->
                                <h3 class="slds-section__title">
                                    <a aria-controls="RegisteredAddressList"
                                        class="slds-button slds-section__title-action" onclick="{!c.toggleSection}"
                                        data-auraId="RegisteredAddress">
                                        <lightning:icon iconName="utility:switch" size="x-small"
                                            class="slds-section__title-action-icon slds-button__icon_left"
                                            alternativeText="button icon" />
                                        <span class="slds-truncate" title="Registered Address">Registered Address</span>
                                    </a>
                                </h3>
                                <div class="slds-section__content" id="RegisteredAddressList">
                                    <div class="slds-grid slds-wrap slds-var-p-around_large">
                                        <!-- Building Code -->
                                        <div
                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_2-of-4 slds-p-bottom_x-small">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label" for="Building Code">
                                                    Building Code</label>
                                                <lightning:inputField aura:id="editFormAccountField"
                                                    variant="label-hidden" id="Building Code"
                                                    fieldName="BuildingCodeLocal__c" name="BuildingCodeLocal__c"
                                                    onchange="{!c.handleAddJsonHeader}" />
                                                <div aura:id="error-BuildingCodeLocal__c"
                                                    class="slds-form-element__help custom-error">
                                                    Complete this field.</div>
                                            </div>
                                        </div>

                                        <!-- Room -->
                                        <div
                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_2-of-4 slds-p-bottom_x-small">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label" for="Room">
                                                    Room</label>
                                                <lightning:inputField aura:id="editFormAccountField"
                                                    variant="label-hidden" id="Room" fieldName="RoomLocal__c"
                                                    name="RoomLocal__c" label="Room"
                                                    onchange="{!c.handleAddJsonHeader}" />
                                                <div aura:id="error-RoomLocal__c"
                                                    class="slds-form-element__help custom-error">Complete
                                                    this field.</div>
                                            </div>
                                        </div>

                                        <!-- Floor -->
                                        <div
                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_2-of-4 slds-p-bottom_x-small">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label" for="Floor">
                                                    Floor</label>
                                                <lightning:inputField aura:id="editFormAccountField"
                                                    variant="label-hidden" id="Floor" fieldName="FloorLocal__c"
                                                    name="FloorLocal__c" onchange="{!c.handleAddJsonHeader}" />
                                                <div aura:id="error-FloorLocal__c"
                                                    class="slds-form-element__help custom-error">Complete
                                                    this field.</div>
                                            </div>
                                        </div>

                                        <!-- c/o -->
                                        <div
                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_2-of-4 slds-p-bottom_x-small">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label" for="c/o">
                                                    c/o</label>
                                                <lightning:inputField aura:id="editFormAccountField"
                                                    variant="label-hidden" id="c/o" fieldName="CoName__c"
                                                    name="CoName__c" onchange="{!c.handleAddJsonHeader}" />
                                                <div aura:id="error-CoName__c"
                                                    class="slds-form-element__help custom-error">
                                                    Complete this
                                                    field.</div>
                                            </div>
                                        </div>

                                        <!-- House no. -->
                                        <div
                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_2-of-4 slds-p-bottom_x-small">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label" for="House no.">
                                                    <abbr class="slds-required" title="required">*</abbr>House
                                                    no.</label>
                                                <lightning:inputField aura:id="editFormAccountField"
                                                    variant="label-hidden" id="House no."
                                                    fieldName="HousenoLocal__c" name="HousenoLocal__c" required="true"
                                                    onchange="{!c.handleAddJsonHeader}" />
                                                <div aura:id="error-HousenoLocal__c"
                                                    class="slds-form-element__help custom-error">
                                                    Complete this field.</div>
                                            </div>
                                        </div>

                                        <!-- Street -->
                                        <div
                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_2-of-4 slds-p-bottom_x-small">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label" for="Street">
                                                    Street</label>
                                                <lightning:inputField aura:id="editFormAccountField"
                                                    variant="label-hidden" id="Street"
                                                    fieldName="StreetLocal__c" name="StreetLocal__c"
                                                    onchange="{!c.handleAddJsonHeader}" />
                                                <div aura:id="error-StreetLocal__c"
                                                    class="slds-form-element__help custom-error">Complete
                                                    this field.</div>
                                            </div>
                                        </div>

                                        <!-- District -->
                                        <div
                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_2-of-4 slds-p-bottom_x-small">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label" for="District">
                                                    District</label>
                                                <lightning:inputField aura:id="editFormAccountField"
                                                    variant="label-hidden" id="District"
                                                    fieldName="DistrictLocalText__c" name="DistrictLocalText__c"
                                                    onchange="{!c.handleAddJsonHeader}" />
                                                <div aura:id="error-DistrictLocalText__c"
                                                    class="slds-form-element__help custom-error">
                                                    Complete this field.</div>
                                            </div>
                                        </div>

                                        <!-- City/State -->
                                        <div
                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_2-of-4 slds-p-bottom_x-small">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label" for="City/State">
                                                    <abbr class="slds-required"
                                                        title="required">*</abbr>City/State</label>
                                                <lightning:inputField aura:id="editFormAccountField"
                                                    variant="label-hidden" id="City/State"
                                                    fieldName="CityStateLocal__c" name="CityStateLocal__c"
                                                    required="true" onchange="{!c.handleAddJsonHeader}" />
                                                <div aura:id="error-CityStateLocal__c"
                                                    class="slds-form-element__help custom-error">
                                                    Complete this field.</div>
                                            </div>
                                        </div>

                                        <!-- Country -->
                                        <div
                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_2-of-4 slds-p-bottom_x-small">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label" for="Country">
                                                    <abbr class="slds-required" title="required">*</abbr>Country
                                                    <lightning:helptext class="custom-help-txt"
                                                        content="This is a sensitive field. If this field changes, the request form must be checked due diligence after submitted." />
                                                </label>
                                                <lightning:inputField aura:id="editFormAccountField"
                                                    variant="label-hidden" id="Country" fieldName="Country__c"
                                                    name="Country__c" required="true"
                                                    onchange="{!c.handleAddJsonHeader}" />
                                                <div aura:id="error-Country__c"
                                                    class="slds-form-element__help custom-error">Complete
                                                    this field.</div>
                                            </div>
                                        </div>

                                        <!-- Postal Code -->
                                        <div
                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_2-of-4 slds-p-bottom_x-small">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label" for="Postal Code">
                                                    Postal Code</label>
                                                <lightning:inputField aura:id="editFormAccountField"
                                                    variant="label-hidden" id="Postal Code"
                                                    fieldName="PostalCodeLocalText__c" name="PostalCodeLocalText__c"
                                                    onchange="{!c.handleAddJsonHeader}" />
                                                <div aura:id="error-PostalCodeLocalText__c"
                                                    class="slds-form-element__help custom-error">
                                                    Complete this field.</div>
                                            </div>
                                        </div>

                                        <!-- Location in Google Maps (Link) -->
                                        <div
                                            class="slds-p-right_medium slds-p-left_medium slds-col slds-size_1-of-2 slds-p-bottom_x-small">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label"
                                                    for="Location in Google Maps (Link)">
                                                    <abbr class="slds-required" title="required">*</abbr>Location in
                                                    Google
                                                    Maps (Link)</label>
