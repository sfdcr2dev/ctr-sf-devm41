<aura:component implements="flexipage:availableForAllPageTypes,force:hasRecordId,force:hasSObjectName" access="global">
    <aura:attribute name="recordId" type="String" />
    <aura:attribute name="sObjectName" type="String" />
    <aura:attribute name="recordTypeId" type="String" />
    <aura:attribute name="recordTypeName" type="String" />
    <aura:attribute name="currentUserName" type="String" />

    <aura:attribute name="recordObject" type="Object" />
    <aura:attribute name="productTypeOptions" type="List" />
    <aura:attribute name="selectedProductType" type="String" />
    <aura:attribute name="isInitialCustomer" type="Boolean" default="false" />
    <aura:attribute name="isCustomer" type="Boolean" default="false" />
    <aura:attribute name="isSupplier" type="Boolean" default="false" />
    <aura:attribute name="recordHeader" type="Object" />
    <aura:attribute name="isOtherProducts" type="Boolean" default="false" />
    <aura:attribute name="SalesOrg" type="String" default="" />
    <aura:attribute name="PurchasingOrg" type="String" default="" />
    <aura:attribute name="interestProductField" type="String" />
    <aura:attribute name="isLoaded" type="Boolean" default="false" />
    <aura:attribute name="isLube" type="Boolean" default="false" />
    <aura:attribute name="isLABIX" type="Boolean" default="false" />
    <aura:attribute name="isPetroluem" type="Boolean" default="false" />
    <aura:attribute name="isOtherProductCustomer" type="Boolean" default="false" />
    <aura:attribute name="isOtherProductSupplier" type="Boolean" default="false" />
    <aura:attribute name="Wth" type="Boolean" default="false" />
    <aura:attribute name="isEdit" type="Boolean" default="false" />
    <aura:attribute name="isExtend" type="Boolean" default="false" />

    <aura:attribute name="ChangedFieldForEDITItem" type="List" default="[]" />

    <!-- <aura:handler name="init" value="{!this}" action="{!c.doInit}" /> -->
    <c:ctrComUtility aura:id="comUtility" onready="{!c.doInit}" onrecordinfo="{!c.handleRecordInfo}"
        onpicklistvalues="{!c.handlePicklistvalues}" />
    <lightning:workspaceAPI aura:id="workspace" />

    <aura:html tag="style">
        .slds-modal__container {
        width: 90% !important;
        max-width: 80% !important;
        height: 100% !important;
        /* width:210mm;
        height:297mm;*/
        }
        .cuf-content {
        padding: 0 0rem !important;
        }
        .slds-p-around--medium {
        padding: 0rem !important;
        }
        .slds-modal__content {
        height: unset !important;
        max-height: unset !important;
        }
    </aura:html>

    <section>

        <lightning:recordEditForm aura:id="recordEditForm" recordId="{!v.recordId}" objectApiName="{!v.sObjectName}"
            onload="{!c.handleLoad}" onsubmit="{!c.handleSubmit}" onsuccess="{!c.handleSuccess}"
            onerror="{!c.handleError}">
            <div class="slds-var-p-around_xx-large slds-size_1-of-1 slds-is-relative slds-text-heading_small">
                <div class="slds-section slds-is-open" aura:id="requestDetailsSection">
                    <h3 class="slds-section__title">
                        <button type="button" aria-controls="requestDetailsDetail" aria-expanded="true"
                            class="slds-button slds-section__title-action" data-auraid="requestDetailsSection"
                            onclick="{!c.handleToggleSection}">
                            <lightning:icon iconName="utility:switch" alternativeText="switch" title="" size="x-small"
                                class="slds-section__title-action-icon slds-button__icon slds-button__icon_left" />
                            <span class="slds-truncate">Request Details</span>
                        </button>
                    </h3>
                    <div class="slds-section__content" id="requestDetailsDetail">
                        <div class="slds-grid slds-wrap slds-var-p-around_large">
                            <div class="slds-col slds-size_1-of-2 slds-var-p-around_medium">
                                <div class="slds-form">
                                    <div class="slds-form-element">
                                        <label class="slds-form-element__label">
                                            Request Number
                                        </label>
                                        <div class="slds-form-element__control">
                                            <lightning:inputField aura:id="inputField" variant="label-hidden"
                                                fieldName="Name" disabled="true" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="slds-col slds-size_1-of-2 slds-var-p-around_medium">
                                <div class="slds-form">
                                    <div class="slds-form-element">
                                        <label class="slds-form-element__label">
                                            Request Type
                                        </label>
                                        <div class="slds-form-element__control">
                                            <lightning:inputField aura:id="inputField" variant="label-hidden"
                                                fieldName="RecordTypeId" disabled="true" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="slds-col slds-size_1-of-2 slds-var-p-around_medium">
                                <div class="slds-form">
                                    <div class="slds-form-element">
                                        <label class="slds-form-element__label">
                                            Counterparty Name
                                        </label>
                                        <div class="slds-form-element__control">
                                            <lightning:inputField aura:id="inputField" variant="label-hidden"
                                                fieldName="Customer__c" disabled="true" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="slds-col slds-size_1-of-2 slds-var-p-around_medium">
                                <div class="slds-form">
                                    <div class="slds-form-element">
                                        <label class="slds-form-element__label">
                                            <abbr class="slds-required" title="required">*</abbr>Objective
                                        </label>
                                        <div class="slds-form-element__control">
                                            <lightning:inputField aura:id="inputField" variant="label-hidden"
                                                fieldName="Objective__c" required="true" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="slds-col slds-size_1-of-2 slds-var-p-around_medium">
                                <div class="slds-form">
                                    <div class="slds-form-element">
                                        <aura:renderIf isTrue="{!v.isCustomer}">
                                            <label class="slds-form-element__label">
                                                <abbr class="slds-required" title="required">*</abbr>Interest Product
                                                Type as Customer
                                            </label>
                                            <div class="slds-form-element__control">
                                                <aura:if isTrue="{!v.isExtend}">
                                                    <lightning:combobox aura:id="inputCustom" variant="label-hidden"
                                                        value="{!v.selectedProductType}" placeholder="--None--"
                                                        options="{!v.productTypeOptions}" required="{!v.isCustomer}"
                                                        name="interestProductTypeAsCustomer"
                                                        onchange="{!c.handleInterestProductCustomerChange}" />
                                                    <aura:set attribute="else">
                                                        <lightning:inputField aura:id="inputCustom"
                                                            variant="label-hidden"
                                                            fieldName="InterestedProductTypeAsCustomerTX__c"
                                                            required="true" />
                                                        <!-- <lightning:combobox aura:id="inputCustom" variant="label-hidden" value="{!v.selectedProductType}" placeholder="-None-" options="{!v.productTypeOptions}" required="{!v.isCustomer}" name="interestProductTypeAsCustomer" /> -->
                                                    </aura:set>
                                                </aura:if>
                                            </div>
                                        </aura:renderIf>
                                        <aura:renderIf isTrue="{!v.isCustomer}">
                                            <label class="slds-form-element__label" style="margin-top: 10px;">
                                                <aura:renderIf isTrue="{!v.isOtherProductCustomer}"><span
                                                        style="color:red; font-style: italic;">* </span></aura:renderIf>
                                                (Customer) Other Products
                                            </label>
                                            <div class="slds-form-element__control">
                                                <lightning:inputField aura:id="inputField" variant="label-hidden"
                                                    fieldName="CustomerProductIdentification__c"
                                                    required="{!v.isOtherProductCustomer}" />
                                            </div>
                                        </aura:renderIf>
                                    </div>
                                </div>
                            </div>
                            <div class="slds-col slds-size_1-of-2 slds-var-p-around_medium">
                                <div class="slds-form">
                                    <div class="slds-form-element">

                                        <aura:renderIf isTrue="{!v.isSupplier}">
                                            <label class="slds-form-element__label">
                                                <abbr class="slds-required" title="required">*</abbr>Interest Product
                                                Type as Supplier
                                            </label>
                                            <div class="slds-form-element__control">
                                                <aura:if isTrue="{!v.isExtend}">
                                                    <lightning:combobox aura:id="inputCustom" variant="label-hidden"
                                                        value="{!v.selectedProductType}" placeholder="--None--"
                                                        options="{!v.productTypeOptions}" required="{!v.isCustomer}"
                                                        name="interestProductTypeAsSupplier"
                                                        onchange="{!c.handleInterestProductCustomerChange}" />
                                                    <aura:set attribute="else">
                                                        <lightning:inputField aura:id="inputCustom"
                                                            variant="label-hidden"
                                                            fieldName="InterestedProductTypeAsSupplierTX__c"
                                                            required="true" />
                                                    </aura:set>
                                                </aura:if>
                                                <!-- <lightning:combobox aura:id="inputCustom" variant="label-hidden" value="{!v.selectedProductType}" placeholder="-None-" options="{!v.productTypeOptions}" required="{!v.isSupplier}" name="interestProductTypeAsSupplier" /> -->
                                            </div>
                                        </aura:renderIf>
                                        <aura:renderIf isTrue="{!v.isSupplier}">
                                            <label class="slds-form-element__label">
                                                <aura:renderIf isTrue="{!v.isOtherProductSupplier}"><span
                                                        style="color:red; font-style: italic;">* </span></aura:renderIf>
                                                (Supplier) Other Products
                                            </label>
                                            <div class="slds-form-element__control">
                                                <lightning:inputField aura:id="inputField" variant="label-hidden"
                                                    fieldName="SupplierProductIdentification__c"
                                                    required="{!v.isOtherProductSupplier}" />
                                            </div>
                                        </aura:renderIf>
                                    </div>
                                </div>
                            </div>
                            <aura:renderIf isTrue="{!v.isEdit}">
                                <div class="slds-col slds-size_1-of-2 slds-var-p-around_medium">
                                    <div class="slds-form">
                                        <div class="slds-form-element">
                                            <label class="slds-form-element__label">
                                                Effective Type
                                            </label>
                                            <div class="slds-form-element__control">
                                                <lightning:inputField aura:id="inputField" variant="label-hidden"
                                                    fieldName="EffectiveType__c" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-col slds-size_1-of-2 slds-var-p-around_medium">
                                    <div class="slds-form">
                                        <div class="slds-form-element">
                                            <label class="slds-form-element__label">
                                                Effective Date
                                            </label>
                                            <div class="slds-form-element__control">
                                                <lightning:inputField aura:id="inputField" variant="label-hidden"
                                                    fieldName="EffectiveDate__c" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </aura:renderIf>
                            <div class="slds-col slds-size_1-of-2 slds-var-p-around_medium">
                                <div class="slds-form">
                                    <div class="slds-form-element">
                                        <!-- <aura:if isTrue="{!v.isCustomer}"> -->
                                        <label class="slds-form-element__label">

                                            Lead Owner (TX Sales Representative)

                                        </label>
                                        <div class="slds-form-element__control">
                                            <lightning:inputField aura:id="inputField" variant="label-hidden"
                                                fieldName="OwnerId" disabled="true" />
                                        </div>
                                        <!-- </aura:if> -->
                                    </div>
                                </div>
                            </div>
                            <div class="slds-col slds-size_1-of-2">

                                <div class="slds-form">
                                    <div class="slds-form-element">
                                        <!-- <aura:if isTrue="{!v.isSupplier}">
                                        <label class="slds-form-element__label">
                                                Lead Owner (Trader Representative)
                                        </label>
                                        <div class="slds-form-element__control">
                                            <lightning:inputField aura:id="inputField" variant="label-hidden" fieldName="OwnerId" disabled="true"/>
                                        </div>
                                    </aura:if> -->
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <aura:renderIf isTrue="{!v.isExtend}">
                    <div class="slds-section slds-is-open" aura:id="SectionContactPersonInformation">
                        <h3 class="slds-section__title">
                            <button aria-controls="SectionContactPersonInformation" aria-expanded="true"
                                class="slds-button slds-section__title-action" onclick="{!c.handleToggleSection}">
                                <lightning:icon iconName="utility:switch" size="x-small"
                                    class="slds-section__title-action-icon slds-button__icon_left" />
                                <span class="slds-truncate" title="Contact Person Information">Contact Person
                                    Information</span>
                            </button>
                        </h3>
                        <div class="slds-section__content" id="SectionContactPersonInformation">
                            <!-- <lightning:recordEditForm aura:id="editFormHeader" recordId="{!v.headerRecordId}" objectApiName="CTRRequestFormItem__c" onload="{!c.handleLoadHeader}"> -->
                            <div class="slds-grid slds-gutters slds-wrap slds-var-p-around_medium">
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-2">
                                    <div class="slds-form slds-var-p-left_x-small slds-var-p-right_x-small">
                                        <div class="slds-form-element">
                                            <label class="slds-form-element__label">Title (Mr./Mrs./Miss/etc.)</label>
                                            <div class="slds-form-element__control">
                                                <lightning:inputField aura:id="editFormItemField"
                                                    fieldName="ContactSalutation__c" variant="label-hidden"
                                                    id="ContactSalutation__c" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-2">
                                    <div class="slds-form">
                                        <div class="slds-form-element">
                                            <div class="slds-form-element__control">
                                                <lightning:inputField aura:id="editFormItemField"
                                                    fieldName="Nickname__c" variant="label-stacked" id="Nickname__c" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-2">
                                    <div class="slds-form">
                                        <div class="slds-form-element">
                                            <div class="slds-form-element__control">
                                                <lightning:inputField aura:id="editFormItemField"
                                                    fieldName="FirstName__c" variant="label-stacked" id="FirstName__c"
                                                    required="true" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-2">
                                    <div class="slds-form">
                                        <div class="slds-form-element">
                                            <div class="slds-form-element__control">
                                                <lightning:inputField aura:id="editFormItemField"
                                                    fieldName="LastName__c" variant="label-stacked" id="LastName__c"
                                                    required="true" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-2">
                                    <div class="slds-form">
                                        <div class="slds-form-element">
                                            <div class="slds-form-element__control">
                                                <lightning:inputField aura:id="editFormItemField"
                                                    fieldName="Position__c" variant="label-stacked" id="Position__c" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-2">
                                    <div class="slds-form">
                                        <div class="slds-form-element">
                                            <div class="slds-form-element__control">
                                                <lightning:inputField aura:id="editFormItemField" fieldName="Email__c"
                                                    variant="label-stacked" id="Email__c" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-2">
                                    <div class="slds-form">
                                        <div class="slds-form-element">
                                            <div class="slds-form-element__control">
                                                <lightning:inputField aura:id="editFormItemField"
                                                    fieldName="MobileCountryCode__c" variant="label-stacked"
                                                    id="MobileCountryCode__c" required="true" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-2">
                                    <div class="slds-form">
                                        <div class="slds-form-element">
                                            <div class="slds-form-element__control">
                                                <lightning:inputField aura:id="editFormItemField"
                                                    fieldName="MobilePhone__c" variant="label-stacked"
                                                    id="MobilePhone__c" required="true" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-2">
                                    <div class="slds-form">
                                        <div class="slds-form-element">
                                            <div class="slds-form-element__control">
                                                <lightning:inputField aura:id="editFormItemField"
                                                    fieldName="PhoneCountryCode__c" variant="label-stacked"
                                                    id="PhoneCountryCode__c" required="true" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-2">
                                    <div class="slds-form">
                                        <div class="slds-form-element">
                                            <div class="slds-form-element__control">
                                                <lightning:inputField aura:id="editFormItemField" fieldName="Phone__c"
                                                    variant="label-stacked" id="Phone__c" required="true" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-2">

                                </div>
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-2">
                                    <div class="slds-form">
                                        <div class="slds-form-element">
                                            <div class="slds-form-element__control">
                                                <lightning:inputField aura:id="editFormItemField"
                                                    fieldName="PhoneExtension__c" variant="label-stacked"
                                                    id="PhoneExtension__c" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-2">
                                    <div class="slds-form">
                                        <div class="slds-form-element">
                                            <div class="slds-form-element__control">
                                                <lightning:inputField aura:id="editFormItemField"
                                                    fieldName="FaxCountryCode__c" variant="label-stacked"
                                                    id="FaxCountryCode__c" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-2">
                                    <div class="slds-form">
                                        <div class="slds-form-element">
                                            <div class="slds-form-element__control">
                                                <lightning:inputField aura:id="editFormItemField" fieldName="Fax__c"
                                                    variant="label-stacked" id="Fax__c" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-2" />
                                <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-2">
                                    <div class="slds-form">
                                        <div class="slds-form-element">
                                            <div class="slds-form-element__control">
                                                <lightning:inputField aura:id="editFormItemField"
                                                    fieldName="FaxExtension__c" variant="label-stacked"
                                                    id="FaxExtension__c" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- </lightning:recordEditForm> -->
                        </div>
                    </div>
                </aura:renderIf>

                <aura:renderIf isTrue="{!v.isCustomer}">
                    <div class="slds-section slds-is-open" aura:id="billingDocumentSection">
                        <h3 class="slds-section__title">
                            <button type="button" aria-controls="billingDocumentDetail" aria-expanded="true"
                                class="slds-button slds-section__title-action" data-auraid="billingDocumentSection"
                                onclick="{!c.handleToggleSection}">
                                <lightning:icon iconName="utility:switch" alternativeText="switch" title=""
                                    size="x-small"
                                    class="slds-section__title-action-icon slds-button__icon slds-button__icon_left" />
                                <span class="slds-truncate">Billing Document</span>
                            </button>
                        </h3>
                        <div class="slds-section__content" id="billingDocumentDetail">
                            <div class="slds-grid slds-wrap slds-var-p-around_large">
                                <div class="slds-col slds-size_1-of-1 slds-var-p-around_medium">
                                    <div class="slds-form">
                                        <div class="slds-form-element">
                                            <div class="slds-form-element__control checkbox-style">
                                                <lightning:inputField aura:id="inputField" variant="label-hidden"
                                                    fieldName="Rebate__c" disabled="true" />
                                                <label class="slds-form-element__label">
                                                    Rebate
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-col slds-size_1-of-1 slds-var-p-around_medium">
                                    <div class="slds-form">
                                        <div class="slds-form-element">
                                            <div class="slds-form-element__control checkbox-style">
                                                <lightning:inputField aura:id="inputField" variant="label-hidden"
                                                    fieldName="IsPriceDetermin__c" disabled="true" />
                                                <label class="slds-form-element__label">
                                                    Price determin.
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-col slds-size_1-of-2 slds-var-p-around_medium">
                                    <div class="slds-form">
                                        <div class="slds-form-element">
                                            <label class="slds-form-element__label">
                                                <abbr class="slds-required" title="required">*</abbr>Credit Control Area
                                            </label>
                                            <div class="slds-form-element__control">
                                                <lightning:inputField aura:id="inputField" id="Credit Control Area"
                                                    variant="label-hidden" fieldName="CreditControlArea__c"
                                                    onchange="{!c.addJsonforEdit}" required="true"
                                                    disabled="{!v.isEdit}" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-col slds-size_1-of-2 slds-var-p-around_medium">
                                    <div class="slds-form">
                                        <div class="slds-form-element">
                                            <label class="slds-form-element__label">
                                                <abbr class="slds-required" title="required">*</abbr>Incoterms
                                            </label>
                                            <div class="slds-form-element__control">
                                                <lightning:inputField aura:id="inputField" id="Incoterms"
                                                    variant="label-hidden" fieldName="Incoterms__c"
                                                    onchange="{!c.addJsonforEdit}" required="true" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
