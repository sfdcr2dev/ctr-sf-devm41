<aura:component
    implements="force:lightningQuickAction,flexipage:availableForAllPageTypes,force:hasRecordId,force:hasSObjectName"
    access="global" controller="CTRSapInformationController">

    <aura:attribute name="requestFormHeaderObj" type="CTRRequestFormHeader__c" default="{}" />
    <aura:attribute name="requestFormItemObj" type="CTRRequestFormItem__c" default="{}" />
    <aura:attribute name="recordTypeName" type="String" />
    <aura:attribute name="currentUserName" type="String" />

    <aura:attribute name="recordId" type="String" />
    <aura:attribute name="recordIdFormHeader" type="String" />

    <aura:attribute name="isModalOpen" type="Boolean" default="false" />
    <aura:attribute name="showLoading" type="Boolean" default="false" />
    <aura:attribute name="recordTypeNameCustomer" type="Boolean" />
    <aura:attribute name="validateMissingField" type="String" />
    <aura:attribute name="validateFieldShow" type="Boolean" default="false" />
    <aura:attribute name="SentGeneralView" type="Boolean" />
    <aura:attribute name="DisabledDistribution" type="Boolean" default="false" />
    <aura:attribute name="loadedValue" type="Boolean" default="true" />


    <aura:attribute name="AccountGroupValue" type="String" />
    <aura:attribute name="DistributionChannelList" type="List" />
    <aura:attribute name="AllDistributionChannelList" type="List" />

    <aura:attribute name="SalesOrgList" type="List" />

    <!-- Picklist tab Sales view     -->
    <aura:attribute name="IncotermsList" type="List" />
    <aura:attribute name="CurrencyList" type="List" />
    <aura:attribute name="TaxClassificationList" type="List" />
    <aura:attribute name="AccountAssignmentGroupList" type="List" />
    <aura:attribute name="PaymentTermList" type="List" />
    <aura:attribute name="PaymentGuaranteeProcedureList" type="List" />
    <aura:attribute name="PriceGroupList" type="List" />
    <aura:attribute name="CompanyCodeTOPList" type="List" />
    <aura:attribute name="PurchasingOrgTOPList" type="List" />
    <aura:attribute name="DivisionList" type="List" />



    <aura:attribute name="ChangedFieldForEDITHeader" type="List" default="[]" />
    <aura:attribute name="ChangedFieldForEDITItem" type="List" default="[]" />

    <aura:attribute name="mDependencyMap" type="Map" default="{}" />
    <aura:handler name="change" value="{!v.AccountGroupValue}" action="{!c.onChangeAccountGroup}" />

    <aura:handler name="OnChangeLookup" event="c:CTROnChangeLookupEvent" action="{!c.LookupChanged}" />


    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />


    <lightning:spinner aura:id="spinner" variant="brand" />
    <lightning:recordEditForm recordId="{!v.recordIdFormHeader}" objectApiName="CTRRequestFormHeader__c"
        onload="{!c.handleLoad}">
        <lightning:messages />
        <div class="slds-modal__content slds-p-around_medium" style="height: 70vh; overflow: auto;">
            <div style="text-align: center;">
            </div>
            <div class="slds-var-m-top_medium slds-var-m-bottom_x-large" style="background-color: white;">
                <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1" style="overflow-y: auto;">
                    <lightning:tabset selectedTabId="generalView">

                        <!-- Check RecordType contains Customer -->
                        <aura:if isTrue="{!v.recordTypeNameCustomer == true}">


                            <!-- Tab General View  -->
                            <lightning:tab label="General View" id="generalView">
                                <div class="slds-grid slds-wrap">
                                    <!-- Information Section -->
                                    <div class="slds-col slds-size_1-of-1">
                                        <div class="slds-section slds-is-open" aura:id="informationSection">
                                            <h3 class="slds-section__title">
                                                <button type="button" aria-controls="informationDetail"
                                                    aria-expanded="true" class="slds-button slds-section__title-action"
                                                    data-auraid="informationSection" onclick="{!c.handleToggleSection}">
                                                    <lightning:icon iconName="utility:switch" alternativeText="switch"
                                                        title="" size="x-small"
                                                        class="slds-section__title-action-icon slds-button__icon slds-button__icon_left" />
                                                    <span class="slds-truncate" title="Information">Information</span>
                                                </button>
                                            </h3>
                                            <div class="slds-section__content" id="informationDetail">
                                                <div class="slds-grid slds-wrap slds-var-p-around_large">
                                                    <!-- Sales Organization -->
                                                    <div
                                                        class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-top_medium">
                                                        <div
                                                            style="text-align: left; padding-left: 50px; display: flex;">
                                                            <!-- <p style="color: red;">* &nbsp;</p> -->
                                                            <p style="color: red;">* &nbsp;</p>Sales Organization
                                                        </div>
                                                    </div>
                                                    <div class="slds-col slds-size_3-of-6"
                                                        style="padding-bottom: 16px; text-align: start;">
                                                        <!-- <lightning:inputField fieldName="SalesOrganizationTOP__c" variant="label-hidden" required="true"  value="{!v.requestFormHeaderObj.SalesOrganizationTOP__c}"  disabled="{!v.SentGeneralView}"/> -->
                                                        <!-- Disable? change to item -->
                                                        <lightning:select name="SalesOrganizationTOP__c" disabled="true"
                                                            variant="label-hidden" label="Sales Organization"
                                                            onchange="{!c.handleAddJsonItem}"
                                                            value="{!v.requestFormItemObj.SalesOrganizationTOP__c}">
                                                            <option value="">Please select Sales Organization
                                                            </option>
                                                            <aura:iteration items="{!v.SalesOrgList}" var="SalesOrgVar">
                                                                <option value="{!SalesOrgVar.value}">
                                                                    {!SalesOrgVar.label}
                                                                </option>
                                                            </aura:iteration>
                                                        </lightning:select>
                                                    </div>
                                                    <!-- Company code -->
                                                    <div
                                                        class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-top_medium">
                                                        <div
                                                            style="text-align: left; padding-left: 50px; display: flex;">
                                                            <!-- <p style="color: red;">* &nbsp;</p> -->
                                                            <p style="color: red;">* &nbsp;</p>Company Code
                                                        </div>
                                                    </div>
                                                    <div class="slds-col slds-size_3-of-6"
                                                        style="padding-bottom: 16px; text-align: start;">
                                                        <!-- <lightning:inputField fieldName="SalesOrganizationTOP__c" variant="label-hidden" required="true"  value="{!v.requestFormHeaderObj.SalesOrganizationTOP__c}"  disabled="{!v.SentGeneralView}"/> -->
                                                        <!-- Disable? change to item -->
                                                        <lightning:select name="SalesOrganizationTOP__c" disabled="true"
                                                            variant="label-hidden" label="Sales Organization"
                                                            onchange="{!c.handleAddJsonItem}"
                                                            value="{!v.requestFormItemObj.SalesOrganizationTOP__c}">
                                                            <option value="">Please select Company Code
                                                            </option>
                                                            <aura:iteration items="{!v.SalesOrgList}" var="SalesOrgVar">
                                                                <option value="{!SalesOrgVar.value}">
                                                                    {!SalesOrgVar.label}
                                                                </option>
                                                            </aura:iteration>
                                                        </lightning:select>
                                                    </div>
                                                    <!-- Account Group -->
                                                    <div
                                                        class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-bottom_medium">
                                                        <div
                                                            style="text-align: left; padding-left: 50px; display: flex;">
                                                            <!-- <p style="color: red;">* &nbsp;</p> -->
                                                            <p style="color: red;">* &nbsp;</p>Account Group
                                                        </div>
                                                    </div>
                                                    <div class="slds-col slds-size_3-of-6"
                                                        style="padding-bottom: 16px; text-align: start;">
                                                        <lightning:inputField fieldName="AccountGroup__c"
                                                            id="Account Group" onchange="{!c.onChangeAccountGroup}"
                                                            variant="label-hidden" required="true"
                                                            value="{!v.requestFormHeaderObj.AccountGroup__c}"
                                                            disabled="{!v.SentGeneralView}" />

                                                    </div>
                                                    <!-- Trading Partner -->
                                                    <div
                                                        class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-bottom_medium">
                                                        <div
                                                            style="text-align: left; padding-left: 62px; display: flex;">
                                                            <!-- <p style="color: red;">* &nbsp;</p> -->
                                                            Trading Partner
                                                        </div>
                                                    </div>
                                                    <div class="slds-col slds-size_3-of-6"
                                                        style="padding-bottom: 16px; text-align: start;">
                                                        <lightning:inputField fieldName="TradingPartner__c" id="Trading Partner"
                                                            onchange="{!c.handleAddJsonHeader}" variant="label-hidden"
                                                            value="{!v.requestFormHeaderObj.TradingPartner__c}"
                                                            disabled="{!v.SentGeneralView}" />
                                                    </div>

                                                    <!-- Industry -->
                                                    <div
                                                        class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-bottom_medium">
                                                        <div
                                                            style="text-align: left; padding-left: 62px; display: flex;">
                                                            <!-- <p style="color: red;">* &nbsp;</p> -->
                                                            Industry
                                                        </div>
                                                    </div>
                                                    <div class="slds-col slds-size_3-of-6"
                                                        style="padding-bottom: 16px; text-align: start;">
                                                        <lightning:inputField fieldName="Industry__c" id="Industry"
                                                            onchange="{!c.handleAddJsonHeader}" variant="label-hidden"
                                                            value="{!v.requestFormHeaderObj.Industry__c}"
                                                            disabled="{!v.SentGeneralView}" />
                                                    </div>
                                                    <!-- Customer Type -->
                                                    <div
                                                        class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-bottom_medium">
                                                        <div
                                                            style="text-align: left; padding-left: 62px; display: flex;">
                                                            <!-- <p style="color: red;">* &nbsp;</p> -->
                                                            Customer Type
                                                        </div>
                                                    </div>
                                                    <div class="slds-col slds-size_3-of-6"
                                                        style="padding-bottom: 16px; text-align: start;">
                                                        <lightning:inputField fieldName="CustomerType__c"
                                                            id="Customer Type" onchange="{!c.handleAddJsonHeader}"
                                                            variant="label-hidden"
                                                            value="{!v.requestFormHeaderObj.CustomerType__c}"
                                                            disabled="{!v.SentGeneralView}" />
                                                        <!-- Backend not mapping -->
                                                    </div>
                                                    <!-- Type of Bussiness (Branch) -->
                                                    <div
                                                        class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-bottom_medium">
                                                        <div
                                                            style="text-align: left; padding-left: 50px; display: flex;">
                                                            <!-- <p style="color: red;">* &nbsp;</p> -->
                                                            <p style="color: red;">* &nbsp;</p>Type of Bussiness (Branch)
                                                        </div>
                                                    </div>
                                                    <div class="slds-col slds-size_3-of-6"
                                                        style="padding-bottom: 16px; text-align: start;">
                                                        <lightning:inputField fieldName="TypeOfBusinessBranch__c"
                                                            id="Type of Bussiness (Branch)"
                                                            onchange="{!c.handleAddJsonHeader}" variant="label-hidden"
                                                            value="{!v.requestFormHeaderObj.TypeOfBusinessBranch__c}"
                                                            disabled="{!v.SentGeneralView}" />
                                                    </div>
                                                    <!-- TAX Number -->
                                                    <div
                                                        class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-bottom_medium">
                                                        <div
                                                            style="text-align: left; padding-left: 50px; display: flex;">
                                                            <!-- <p style="color: red;">* &nbsp;</p> -->
                                                            <p style="color: red;">* &nbsp;</p>TAX Number
                                                        </div>
                                                    </div>
                                                    <div class="slds-col slds-size_3-of-6"
                                                        style="padding-bottom: 16px; text-align: start;">
                                                        <lightning:inputField fieldName="TaxCode1__c" id="TAX Number"
                                                            onchange="{!c.handleAddJsonHeader}" variant="label-hidden"
                                                            required="true"
                                                            value="{!v.requestFormHeaderObj.TaxCode1__c}"
                                                            disabled="true" />
                                                    </div>

                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <!-- Name Section -->
                                    <div class="slds-col slds-size_1-of-1">
                                        <div class="slds-section slds-is-open" aura:id="nameSection">
                                            <h3 class="slds-section__title">
                                                <button type="button" aria-controls="nameDetail" aria-expanded="true"
                                                    class="slds-button slds-section__title-action"
                                                    data-auraid="nameSection" onclick="{!c.handleToggleSection}">
                                                    <lightning:icon iconName="utility:switch" alternativeText="switch"
                                                        title="" size="x-small"
                                                        class="slds-section__title-action-icon slds-button__icon slds-button__icon_left" />
                                                    <span class="slds-truncate" title="Name">Name</span>
                                                </button>
                                            </h3>

                                            <div class="slds-section__content" id="nameDetail">
                                                <div class="slds-grid slds-wrap slds-var-p-around_large">
                                                    <!-- Full Name * -->
                                                    <div
                                                        class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-bottom_medium">
                                                        <div
                                                            style="text-align: left; padding-left: 62px; display: flex;">
                                                            <!-- <p style="color: red;">* &nbsp;</p> -->
                                                            Full Name
                                                        </div>
                                                    </div>
                                                    <div class="slds-col slds-size_3-of-6"
                                                        style="padding-bottom: 16px; text-align: start;">
                                                        {!v.requestFormHeaderObj.CompanyNameLocal__c}
                                                    </div>
                                                    <!-- Name * -->
                                                    <div
                                                        class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-bottom_medium">
                                                        <div
                                                            style="text-align: left; padding-left: 50px; display: flex;">
                                                            <!-- <p style="color: red;">* &nbsp;</p> -->
                                                            <p style="color: red;">* &nbsp;</p>Name
                                                        </div>
                                                    </div>
                                                    <div class="slds-col slds-size_3-of-6"
                                                        style="padding-bottom: 16px; text-align: start;">
                                                        <lightning:inputField fieldName="CustomerNameLocal1__c"
                                                            variant="label-hidden" required="true" id="Name (Local 1)"
                                                            onchange="{!c.handleAddJsonHeader}"
                                                            value="{!v.requestFormHeaderObj.CustomerNameLocal1__c}"
                                                            disabled="{!v.SentGeneralView}" />

                                                    </div>
                                                    <!--  Name input 2  -->
                                                    <div
                                                        class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-bottom_medium">
                                                        <p style="text-align: left; padding-left: 60px; display: flex;">
                                                            <!-- {!v.requestFormHeaderObj.CompanyNameLocal__c} -->
                                                        </p>
                                                    </div>
                                                    <div class="slds-col slds-size_3-of-6"
                                                        style="padding-bottom: 16px; text-align: start;">
                                                        <lightning:inputField fieldName="CustomerNameLocal2__c"
                                                            variant="label-hidden" id="Name (Local 2)"
                                                            onchange="{!c.handleAddJsonHeader}"
                                                            value="{!v.requestFormHeaderObj.CustomerNameLocal2__c}"
                                                            disabled="{!v.SentGeneralView}" />

                                                    </div>
                                                    <!-- Full Name EN* -->
                                                    <div
                                                        class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-bottom_medium">
                                                        <div
                                                            style="text-align: left; padding-left: 62px; display: flex;">
                                                            <!-- <p style="color: red;">* &nbsp;</p> -->
                                                            Full Name (EN)
                                                        </div>
                                                    </div>
                                                    <div class="slds-col slds-size_3-of-6"
                                                        style="padding-bottom: 16px; text-align: start;">
                                                        {!v.requestFormHeaderObj.CompanyNameENF__c}
                                                    </div>
                                                    <!-- Name(Eng) * -->
                                                    <div
                                                        class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-bottom_medium">
                                                        <div
                                                            style="text-align: left; padding-left: 62px; display: flex;">
                                                            Name(Eng)
                                                        </div>

                                                    </div>
                                                    <div class="slds-col slds-size_3-of-6"
                                                        style="padding-bottom: 16px; text-align: start;">
                                                        <lightning:inputField fieldName="CustomerNameEN1__c"
                                                            variant="label-hidden" id="Name(Eng 1)"
                                                            onchange="{!c.handleAddJsonHeader}"
                                                            value="{!v.requestFormHeaderObj.CustomerNameEN1__c}"
                                                            disabled="{!v.SentGeneralView}" />

                                                    </div>
                                                    <!--  name(Eng) input 2 -->
                                                    <div
                                                        class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-bottom_medium">
                                                        <p style="text-align: left; padding-left: 62px; display: flex;">
                                                            <!-- {!v.requestFormHeaderObj.CompanyNameENF__c} -->
                                                        </p>
                                                    </div>
                                                    <div class="slds-col slds-size_3-of-6"
                                                        style="padding-bottom: 16px; text-align: start;">
                                                        <lightning:inputField fieldName="CustomerNameEN2__c"
                                                            variant="label-hidden" id="Name (EN 2)"
                                                            onchange="{!c.handleAddJsonHeader}"
                                                            value="{!v.requestFormHeaderObj.CustomerNameEN2__c}"
                                                            disabled="{!v.SentGeneralView}" />

                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- Search Terms Section -->
                                    <div class="slds-col slds-size_1-of-1">
                                        <div class="slds-section slds-is-open" aura:id="searchTermsSection">
                                            <h3 class="slds-section__title">
                                                <button type="button" aria-controls="searchTermsDetail"
                                                    aria-expanded="true" class="slds-button slds-section__title-action"
                                                    data-auraid="searchTermsSection" onclick="{!c.handleToggleSection}">
                                                    <lightning:icon iconName="utility:switch" alternativeText="switch"
                                                        title="" size="x-small"
                                                        class="slds-section__title-action-icon slds-button__icon slds-button__icon_left" />
                                                    <span class="slds-truncate" title="Search Terms">Search Terms</span>
                                                </button>
                                            </h3>
                                            <div class="slds-section__content" id="searchTermsDetail">
                                                <div class="slds-grid slds-wrap slds-var-p-around_large">
                                                    <!-- Search Term 1* -->
                                                    <div
                                                        class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-top_medium">
                                                        <div
                                                            style="text-align: left; padding-left: 50px; display: flex;">
                                                            <!-- <p style="color: red;">* &nbsp;</p> -->
                                                            <p style="color: red;">* &nbsp;</p>Search Term 1
                                                        </div>
                                                    </div>
                                                    <div class="slds-col slds-size_3-of-6"
                                                        style="padding-bottom: 16px; text-align: start;">
                                                        <lightning:inputField fieldName="CustomerSearchTermLocal__c"
                                                            variant="label-hidden" required="true"
                                                            id="Search Term 1 (Local)"
                                                            onchange="{!c.handleAddJsonHeader}"
                                                            value="{!v.requestFormHeaderObj.CustomerSearchTermLocal__c}"
                                                            disabled="{!v.SentGeneralView}" />

                                                    </div>
                                                    <!-- Search Term 1(Eng) -->
                                                    <div
                                                        class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-top_medium">
                                                        <p style="text-align: left; padding-left: 62px; display: flex;">
                                                            Search Term 1(Eng)
                                                        </p>
                                                    </div>
                                                    <div class="slds-col slds-size_3-of-6"
                                                        style="padding-bottom: 16px; text-align: start;">
                                                        <lightning:inputField fieldName="CustomerSearchTermEN__c"
                                                            id="Search Term 1 (EN)" onchange="{!c.handleAddJsonHeader}"
                                                            variant="label-hidden"
                                                            value="{!v.requestFormHeaderObj.CustomerSearchTermEN__c}"
                                                            disabled="{!v.SentGeneralView}" />
                                                    </div>
                                                    <!-- Street/House number* -->
                                                    <div
                                                        class="slds-col slds-size_4-of-12 slds-p-horizontal_x-small slds-p-top_medium">
                                                        <p style="text-align: left; padding-left: 50px; display: flex;">
                                                        <p style="color: red;">* &nbsp;</p>Street/House number
                                                        </p>

                                                    </div>
                                                    <div class="slds-col slds-size_4-of-12"
                                                        style="padding-bottom: 16px; text-align: start;">
                                                        <lightning:inputField fieldName="StreetLocal__c"
                                                            variant="label-hidden" required="true" id="Street (Local)"
                                                            onchange="{!c.handleAddJsonHeader}"
                                                            value="{!v.requestFormHeaderObj.StreetLocal__c}"
                                                            disabled="{!v.SentGeneralView}" />

                                                    </div>
                                                    <div class="slds-col slds-size_2-of-12"
                                                        style="padding-left: 10px; padding-bottom: 16px;">
                                                        <lightning:inputField fieldName="HousenoLocal__c"
                                                            variant="label-hidden" id="House number"
                                                            onchange="{!c.handleAddJsonHeader}"
                                                            value="{!v.requestFormHeaderObj.HousenoLocal__c}"
                                                            disabled="{!v.SentGeneralView}" />

                                                    </div>
                                                    <!-- row 1* -->
                                                    <div
                                                        class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-top_medium">
                                                        <p style="text-align: left; padding-left: 50px; display: flex;">
                                                        </p>
                                                    </div>
                                                    <div class="slds-col slds-size_3-of-6"
                                                        style="padding-bottom: 16px; text-align: start;">
                                                        <lightning:inputField fieldName="Street2Local__c"
                                                            variant="label-hidden" id="Street2 (Local)"
                                                            onchange="{!c.handleAddJsonHeader}"
                                                            value="{!v.requestFormHeaderObj.Street2Local__c}"
                                                            disabled="{!v.SentGeneralView}" />

                                                    </div>
                                                    <!-- row 2* -->
                                                    <div
                                                        class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-top_medium">
                                                        <p style="text-align: left; padding-left: 50px; display: flex;">
                                                        </p>
                                                    </div>
                                                    <div class="slds-col slds-size_3-of-6"
                                                        style="padding-bottom: 16px; text-align: start;">
                                                        <lightning:inputField fieldName="Street3Local__c"
                                                            variant="label-hidden" id="Street3 (Local)"
                                                            onchange="{!c.handleAddJsonHeader}"
                                                            value="{!v.requestFormHeaderObj.Street3Local__c}"
                                                            disabled="{!v.SentGeneralView}" />

                                                    </div>
                                                    <!-- row 3* -->
                                                    <div
                                                        class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-top_medium">
                                                        <p style="text-align: left; padding-left: 50px; display: flex;">
                                                        </p>
                                                    </div>
                                                    <div class="slds-col slds-size_3-of-6"
                                                        style="padding-bottom: 16px; text-align: start;">
                                                        <lightning:inputField fieldName="Street4Local__c"
                                                            variant="label-hidden" id="Street4 (Local)"
                                                            onchange="{!c.handleAddJsonHeader}"
                                                            value="{!v.requestFormHeaderObj.Street4Local__c}"
                                                            disabled="{!v.SentGeneralView}" />

                                                    </div>
                                                    <!-- row 4* -->
                                                    <div
                                                        class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-top_medium">
                                                        <p style="text-align: left; padding-left: 50px; display: flex;">
                                                        </p>
                                                    </div>
                                                    <div class="slds-col slds-size_3-of-6"
