<aura:component
    implements="lightning:hasPageReference,lightning:actionOverride,force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction"
    access="global" controller="CTRLeadOverideNewController">

    <aura:handler name="change" value="{!v.pageReference}" action="{!c.onPageReferenceChanged}" />

    <aura:attribute name="ObjectType" type="String" default="Lead" />
    <aura:attribute name="onPageRecordID" type="String" />
    <aura:attribute name="recordId" type="String" />
    <aura:attribute name="recordTypeId" type="String" />

    <aura:attribute name="editRecord" type="Boolean" />
    <aura:attribute name="showUnqualifiedReason" type="Boolean" default="false" />
    <aura:attribute name="showOtherUnqualifiedReason" type="Boolean" default="false" />
    <aura:attribute name="showLoading" type="Boolean" default="false" />
    <aura:attribute name="showDuplicate" type="Boolean" default="false" />
    <aura:attribute name="columns" type="List" />
    <aura:attribute name="duplicateRecords" type="List" />
    <aura:attribute name="sortDirection" type="String" default="asc" />
    <aura:attribute name="defaultSortDirection" type="String" default="asc" />
    <aura:attribute name="sortedBy" type="String" />

    <aura:handler name="init" action="{!c.doInit}" value="{!this}" />

    <lightning:workspaceAPI aura:id="workspace"/>
    <div class="card">
        <lightning:layoutItem padding="around-small">
            <aura:if isTrue="{!v.showLoading}">
                <lightning:spinner alternativeText="Loading" size="medium" />
            </aura:if>
            <lightning:recordEditForm aura:id="recordEditForm" recordId="{!v.onPageRecordID}"
                objectApiName="{!v.ObjectType}" recordTypeId="{!v.recordTypeId}" onsuccess="{!c.handleSuccess}" onerror="{!c.handleError}">
                <!--Initial Information section header-->
                <div class="slds-section slds-is-open animate" aura:id="InitialInformation">
                    <h3 class="slds-section__title">
                        <button type="button" aria-controls="InitialInformationList" class="slds-button slds-section__title-action"
                            onclick="{!c.toggleSection}" data-auraId="InitialInformation">
                            <lightning:icon iconName="utility:switch" size="x-small"
                                class="slds-section__title-action-icon slds-button__icon_left"
                                alternativeText="button icon" />
                            <span class="slds-truncate" title="Counterparty Information">Initial Information</span>
                        </button>
                    </h3>
                    <!--section body-->
                    <div class="slds-section__content" id="InitialInformationList">
                        <div class="slds-grid slds-wrap">

                            <!-- row 1 -->
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label><span style="color:red; font-style: italic;">* </span>Lead Source</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="LeadSource" fieldName="LeadSource" variant="label-hidden"
                                    required="true" />
                            </div>
                            <div class="slds-hide">
                                <lightning:inputField aura:id="RegisteredasCustomer__c" fieldName="RegisteredasCustomer__c" variant="label-hidden"
                                    value="{!true}" required="true" />
                            </div>
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label><span style="color:red; font-style: italic;">* </span>Lead Status</label>

                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="Status" fieldName="Status" variant="label-hidden"
                                    required="true" onchange="{!c.onchangeStatus}"/>
                            </div>
                            <div class="slds-col slds-size_12-of-12" style="height: 16px;"></div>
                            <!-- row 2 -->
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>Referral By</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="ReferralBy__c" fieldName="ReferralBy__c"
                                    variant="label-hidden" />
                            </div>
                            <!-- Lead Status == Unqualified -->
                            <aura:if isTrue="{!v.showUnqualifiedReason == true}">
                                <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                    <label>Unqualified reason</label>
                                </div>
                                <div class="slds-col slds-size_4-of-12">

                                    <lightning:inputField aura:id="UnqualifiedReason__c"
                                        fieldName="UnqualifiedReason__c" variant="label-hidden"
                                         onchange="{!c.onchangeUnqualifiedReason}"/>

                                </div>
                            </aura:if>
                            <div class="slds-col slds-size_12-of-12" style="height: 16px;"></div>
                            <!-- row 3 -->
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>Seller</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="Seller__c" fieldName="Seller__c" variant="label-hidden" />
                            </div>
                            <!-- Unqualified reason == Other -->
                            <aura:if isTrue="{!v.showOtherUnqualifiedReason == true}">
                                <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                    <label>Other Unqualified Reason</label>
                                </div>
                                <div class="slds-col slds-size_4-of-12">
                                    <lightning:inputField aura:id="OtherUnqualifiedReason__c"
                                        fieldName="OtherUnqualifiedReason__c" variant="label-hidden" 
                                         />
                                </div>
                            </aura:if>
                        </div>
                    </div>
                </div>

                <!--Lead Owner section header-->
                <div class="slds-section slds-is-open animate" aura:id="LeadOwner">
                    <h3 class="slds-section__title">
                        <button type="button" aria-controls="LeadOwnerList" class="slds-button slds-section__title-action"
                            onclick="{!c.toggleSection}" data-auraId="LeadOwner">
                            <lightning:icon iconName="utility:switch" size="x-small"
                                class="slds-section__title-action-icon slds-button__icon_left"
                                alternativeText="button icon" />
                            <span class="slds-truncate" title="Lead Owner">Lead Owner</span>
                        </button>
                    </h3>
                    <!--section body-->
                    <div class="slds-section__content" id="LeadOwnerList">
                        <div class="slds-grid slds-wrap">
                            <!-- row 1 -->
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>Section of Lead Owner</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="SectionofLeadOwner__c" fieldName="SectionofLeadOwner__c" variant="label-hidden" disabled="true"/>
                            </div>
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>Lead Owner</label>

                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:outputField aura:id="OwnerId" fieldName="OwnerId" variant="label-hidden" />
                            </div>
                        </div>

                    </div>
                </div>

                <!--Company Information section header-->
                <div class="slds-section slds-is-open animate" aura:id="CompanyInformation">
                    <h3 class="slds-section__title">
                        <button type="button" aria-controls="CompanyInformationList" class="slds-button slds-section__title-action"
                            onclick="{!c.toggleSection}" data-auraId="CompanyInformation">
                            <lightning:icon iconName="utility:switch" size="x-small"
                                class="slds-section__title-action-icon slds-button__icon_left"
                                alternativeText="button icon" />
                            <span class="slds-truncate" title="Company Information ">Company Information </span>
                        </button>
                    </h3>
                    <!--section body-->
                    <div class="slds-section__content" id="CompanyInformationList">
                        <div class="slds-grid slds-wrap">
                            <!-- row 1 -->
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label><span style="color:red; font-style: italic;">* </span>Company
                                    Name(English)</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="Company" fieldName="Company" variant="label-hidden"
                                    required="true" />
                            </div>
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;"></div>
                            <div class="slds-col slds-size_4-of-12"></div>
                            <div class="slds-col slds-size_12-of-12" style="height: 16px;"></div>

                            <!-- row 2 -->
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>Company Name(Other Languages)</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="CompanyNameOtherLanguages__c" fieldName="CompanyNameOtherLanguages__c" variant="label-hidden" />
                            </div>
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>Specify Language of Company Name</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="SpecifyLanguageofCompanyName__c" fieldName="SpecifyLanguageofCompanyName__c" variant="label-hidden" />
                            </div>
                            <div class="slds-col slds-size_12-of-12" style="height: 16px;"></div>

                            <!-- row 3 -->
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>Former Name of company</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="FormerNameofCompany__c"
                                    fieldName="FormerNameofCompany__c" variant="label-hidden" />
                            </div>
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label><span style="color:red; font-style: italic;">* </span>Country</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="Country__c" fieldName="Country__c" variant="label-hidden"
                                    required="true" />
                            </div>
                            <div class="slds-col slds-size_12-of-12" style="height: 16px;"></div>

                            <!-- row 4 -->
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>TAX Number</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="TAXNumber__c"
                                    fieldName="TAXNumber__c" variant="label-hidden" />
                            </div>
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>Overview</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="Overview__c" fieldName="Overview__c" variant="label-hidden"
                                     />
                            </div>
                            <div class="slds-col slds-size_12-of-12" style="height: 16px;"></div>

                            <!-- row 5 -->
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>Company Profile</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="CompanyProfile__c"
                                    fieldName="CompanyProfile__c" variant="label-hidden" />
                            </div>
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>Business Background</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="BusinessBackground__c" fieldName="BusinessBackground__c" variant="label-hidden"
                                     />
                            </div>
                            <div class="slds-col slds-size_12-of-12" style="height: 16px;"></div>

                        </div>

                    </div>
                </div>

                <!--Product Type of Interested section header-->
                <div class="slds-section slds-is-open animate" aura:id="ProductTypeofInterested">
                    <h3 class="slds-section__title">
                        <button type="button" aria-controls="ProductTypeofInterestedList" class="slds-button slds-section__title-action"
                            onclick="{!c.toggleSection}" data-auraId="ProductTypeofInterested">
                            <lightning:icon iconName="utility:switch" size="x-small"
                                class="slds-section__title-action-icon slds-button__icon_left"
                                alternativeText="button icon" />
                            <span class="slds-truncate" title="Product Type of Interested">Product Type of Interested</span>
                        </button>
                    </h3>
                    <!--section body-->
                    <div class="slds-section__content" id="ProductTypeofInterestedList">
                        <div class="slds-grid slds-wrap">
                            <!-- row 1 -->
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label><span style="color:red; font-style: italic;">* </span>Interested Product Type as Customer</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="InterestedProductTypeAsCustomerTX__c" fieldName="InterestedProductTypeAsCustomerTX__c" 
                                variant="label-hidden" required="true"/>

                                <!-- <lightning:inputField aura:id="InterestedProductTypeasCustomer__c" fieldName="InterestedProductTypeasCustomer__c" variant="label-hidden" /> -->
                            </div>
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>Customer Product Identification</label>

                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="CustomerProductIdentification__c" fieldName="CustomerProductIdentification__c" variant="label-hidden" />
                            </div>
                            <div class="slds-col slds-size_12-of-12" style="height: 16px;"></div>

                            <!-- row 2 -->
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>CustomerType</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="CustomerType__c" fieldName="CustomerType__c" variant="label-hidden" />
                            </div>
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                            </div>
                            <div class="slds-col slds-size_12-of-12" style="height: 16px;"></div>

                            <!-- row 2 -->
                            <div class="slds-col slds-size_12-of-12" style="text-align: center; margin-top: 5px; margin-left: 16px;">
                                <span style="display: flex;"><p style="font-weight: bold;">Product Group Name</p></span>
                                <span style="display: flex;"><p style="font-weight: bold;">Fuel: </p> Fuel Oil, Gasoll, Gasoline, Jet, Kerosene, LPG, Naphtha. Sulphur, VGO, HCB/HIV </span>
                                <span style="display: flex;"><p style="font-weight: bold;">Lube: </p> Base OIL Bitumen, Extract, MES, Slack Wax, TDAE. TRAE</span>
                                <span style="display: flex;"><p style="font-weight: bold;">Aromatic: </p> Benzene, MX. PX. Toluene</span>
                                <span style="display: flex;"><p style="font-weight: bold;">LAB: </p> HAB, LAB</span>
                                <span style="display: flex;"><p style="font-weight: bold;">Solvents  Chemical: </p> Chemical Solvents, Hydrocarbon Solvents linclude nP(1), Raffinate(2).</span>
                                <span style="display: flex;">Other ChemicalsOlefins: Polymer, Monomer, Pygas, Ethylene cracker bottom oil (EBO)</span>
                                <!--<a style="display: flex;" href=""> Product Group Name</a>
                                <div ><img class="imgBdr" src="{!$Resource.TopGroupProductHierarchy}"  /> </div>-->
                               <a href="{!$Resource.CTRTopGroupProductHierarchy}" target="_blank">Product Group Name</a>


                            </div>
                        </div>

                    </div>
                </div>

                <!--Registered Address header-->
                <div class="slds-section slds-is-open animate" aura:id="RegisteredAddress">
                    <h3 class="slds-section__title">
                        <button type="button" aria-controls="RegisteredAddressList" class="slds-button slds-section__title-action"
                            onclick="{!c.toggleSection}" data-auraId="RegisteredAddress">
                            <lightning:icon iconName="utility:switch" size="x-small"
                                class="slds-section__title-action-icon slds-button__icon_left"
                                alternativeText="button icon" />
                            <span class="slds-truncate" title="Registered Address">Registered Address</span>
                        </button>
                    </h3>
                    <!--section body-->
                    <div class="slds-section__content" id="RegisteredAddressList">
                        <div class="slds-grid slds-wrap">
                            <!-- row 1 -->
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>House no.</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="HouseNo__c" fieldName="HouseNo__c"
                                    variant="label-hidden" />
                            </div>
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>Building Code</label>

                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="BuildingCode__c" fieldName="BuildingCode__c"
                                    variant="label-hidden" />
                            </div>
                            <div class="slds-col slds-size_12-of-12" style="height: 16px;"></div>

                            <!-- row 2 -->
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>Room</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="Room__c" fieldName="Room__c" variant="label-hidden" />
                            </div>
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>Floor</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="Floor__c" fieldName="Floor__c" variant="label-hidden" />
                            </div>
                            <div class="slds-col slds-size_12-of-12" style="height: 16px;"></div>

                            <!-- row 3 -->
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>Street</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="Street__c" fieldName="Street__c"
                                    variant="label-hidden" />
                            </div>
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                            </div>
                            <div class="slds-col slds-size_12-of-12" style="height: 16px;"></div>

                            <!-- row 4 -->
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>Sub District</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="SubDistrict__c" fieldName="SubDistrict__c"
                                    variant="label-hidden" />
                            </div>
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>District</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="District__c" fieldName="District__c"
                                    variant="label-hidden" />
                            </div>
                            <div class="slds-col slds-size_12-of-12" style="height: 16px;"></div>

                            <!-- row 5 -->
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>City/State</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="CityState__c" fieldName="CityState__c"
                                    variant="label-hidden" />
                            </div>
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>Postal Code</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="PostalCode__c" fieldName="PostalCode__c"
                                    variant="label-hidden" />
                            </div>
                            <div class="slds-col slds-size_12-of-12" style="height: 16px;"></div>
                        </div>

                    </div>
                </div>

                <!--Contact Person section header-->
                <div class="slds-section slds-is-open animate" aura:id="ContactPerson">
                    <h3 class="slds-section__title">
                        <button type="button" aria-controls="ContactPersonList" class="slds-button slds-section__title-action"
                            onclick="{!c.toggleSection}" data-auraId="ContactPerson">
                            <lightning:icon iconName="utility:switch" size="x-small"
                                class="slds-section__title-action-icon slds-button__icon_left"
                                alternativeText="button icon" />
                            <span class="slds-truncate" title="Contact Person">Contact Person</span>
                        </button>
                    </h3>
                    <!--section body-->
                    <div class="slds-section__content" id="ContactPersonList">
                        <div class="slds-grid slds-wrap">
                            <!-- row 1 -->
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>Title (Mr./Mrs./Miss)</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="Title" fieldName="Title" variant="label-hidden" />
                            </div>
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label>Nickname</label>

                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="Nickname__c" fieldName="Nickname__c" variant="label-hidden" />
                            </div>
                            <div class="slds-col slds-size_12-of-12" style="height: 16px;"></div>

                            <!-- row 2 -->
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label><span style="color:red; font-style: italic;">* </span>First Name</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
                                <lightning:inputField aura:id="FirstName" fieldName="FirstName" variant="label-hidden"
                                    required="true" />
                            </div>
                            <div class="slds-col slds-size_2-of-12" style="text-align: center; margin-top: 5px;">
                                <label><span style="color:red; font-style: italic;">* </span>Last Name</label>
                            </div>
                            <div class="slds-col slds-size_4-of-12">
