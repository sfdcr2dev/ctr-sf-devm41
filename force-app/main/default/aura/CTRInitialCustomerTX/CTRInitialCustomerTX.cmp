                                            <span style="color:red; font-style: italic;">* </span>
                                            Account Group
                                        </label>
                                        <lightning:inputField aura:id="AccountGroup__c" fieldName="AccountGroup__c"
                                            variant="label-hidden" required="true" 
                                            id="Account Group"
                                            onchange="{!c.handleisAccountGroupChange}" disabled="{!v.SAPSync}"/>
                                        <br />
                                        
                                            <label><span style="color:red; font-style: italic;">* </span>Group Key (Account  Key of Bank)</label>
                                            <lightning:inputField aura:id="GroupKeyAccountKeyofBank__c" fieldName="GroupKeyAccountKeyofBank__c"
                                                variant="label-hidden" 
                                                id="Group Key (Account  Key of Bank)"
                                                onchange="{!c.handleAddJson}" disabled="{!v.SAPSync}" required="true" />
                                            <br />
                                    </div>
                                    <div class="slds-col slds-size_1-of-4"></div>
                                    <div class="slds-col slds-size_1-of-4">
                                        <!-- Your lightning:inputField components here Right Col -->
                                        <label>
                                            <aura:renderIf isTrue="{!v.isAccountGroup}">
                                                <span style="color:red; font-style: italic;">* </span>
                                            </aura:renderIf>
                                            Trading Partner
                                        </label>
                                        <lightning:inputField aura:id="TradingPartner__c" fieldName="TradingPartner__c"
                                            variant="label-hidden" 
                                            id="Trading Partner"
                                            onchange="{!c.handleAddJson}" 
                                            required="{!v.isAccountGroup}"
                                            disabled="{!v.SAPSync}"/>
                                        <br />
                                        
                                    </div>
                                    <div class="slds-col slds-size_1-of-4"></div>

                                </div>
                            </div>
                        </div>
                    </aura:renderIf>

                    <div class="slds-align_absolute-center" style="height:5rem">
                        <lightning:button class="slds-m-top_small" variant="brand" type="cancel" name="cancel"
                            label="Cancel" onclick="{! c.handleCancel}" />
                        <lightning:button class="slds-m-top_small" variant="brand" type="submit" name="save"
                            label="Save" />
                    </div>

                </lightning:recordEditForm>
            </lightning:layoutItem>
        </div>
        <!--
                </div>
            </div>
        </div>-->
        <!--<div aura:id="overlay" class="slds-backdrop slds-backdrop-open" />https://github.com/johnsfdemo/NewActionOverride/blob/master/src/aura/NewActionOverride/NewActionOverride.cmp -->
        <aura:if isTrue="{!v.isModalSensitiveOpen}">
            <!-- Modal/Popup Box starts here-->
            <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
                <div class="slds-modal__container custom-modal-width">
                    <!-- Modal/Popup Box Header Starts here-->
                    <header class="slds-modal__header">
                        <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">Edit Information</h2>
                    </header>
                    <!--Modal/Popup Box Body Starts here-->
                    <div class="slds-modal__content slds-p-around_medium slds-text-align_center" id="modal-content-id-1">
                        <p>We found sensitive fields have been edited</p>
                        <p>Your request number: <b>{!v.reqItemNumber}</b> has been created.</p>
                        <p>Please do not forget to upload reference documents if any.</p>
                    </div>
                    <!--Modal/Popup Box Footer Starts here-->
                    <footer class="slds-modal__footer slds-text-align_center">
                        <lightning:button variant="brand"
                                        label="OK"
                                        title="OK"
                                        onclick="{!c.closeModel}"/>
                    </footer>
                </div>
            </section>
            <div class="slds-backdrop slds-backdrop_open"></div>
        </aura:if>

    </aura:renderIf>
    <aura:if isTrue="{!!v.isLoaded}">
        <lightning:spinner variant="brand" style="z-index:2" />
    </aura:if>
</aura:component>