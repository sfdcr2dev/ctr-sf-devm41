<aura:component controller="CTRInitialCustomerTOP"
    implements="lightning:hasPageReference,lightning:actionOverride,force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction"
    access="global">
    <aura:attribute name="options" type="List" default="[
                                                        {'label': 'National Oil Company', 'value': 'National Oil Company'},
                                                        {'label': 'End User', 'value': 'End User'},
                                                        {'label': 'Trader &amp; Distributor', 'value': 'Trader &amp; Distributor'},
                                                        {'label': 'Other (Please specify)', 'value': 'Other (Please specify)'}
                                                        ]" />
    <aura:handler name="change" value="{!v.pageReference}" action="{!c.onPageReferenceChanged}" />

    <aura:attribute name="value" type="String" default="{!v.typeOfBusiness}" />

    <aura:attribute name="shareholderList" type="CTRShareholder__c[]" />
    <aura:attribute name="shippingList" type="CTRShippingCountry__c[]" />

    <aura:attribute name="ChangedFieldForEDIT" type="List" default="[]"/>

    <aura:handler name="CTRAddNewRowEvt" event="c:CTRAddNewRowEvt" action="{!c.addNewRow}" />
    <aura:handler name="CTRDeleteRowEvt" event="c:CTRDeleteRowEvt" action="{!c.removeDeletedRow}" />

    <aura:handler name="CTRAddNewRowDestinationEvt" event="c:CTRAddNewRowDestinationEvt"
        action="{!c.addNewRowDestination}" />
    <aura:handler name="CTRDeleteRowDestinationEvt" event="c:CTRDeleteRowDestinationEvt"
        action="{!c.removeDeletedRowDestination}" />

    <aura:handler name="init" action="{!c.doInit}" value="{!this}" />
    <lightning:navigation aura:id="navService" />

    <aura:attribute name="recordTypeId" type="String" />
    <aura:attribute name="typeOfBusiness" type="String" />
    <aura:attribute name="recordTypeName" type="String" />
    <aura:attribute name="currentUserName" type="String" />
    <aura:attribute name="reqItemNumber" type="String" />

    <aura:attribute name="language" type="String[]" />

    <aura:attribute name="country" type="String[]" />
    <aura:attribute name="location" type="String[]" />
    <aura:attribute name="FacilityTransportation" type="String[]" />
    <aura:attribute name="FacilityTankAndStorage" type="String[]" />

    <aura:attribute name="CustomerTankAndStorageValue" type="List" default="" />
    <aura:attribute name="SupplierTankAndStorageValue" type="List" default="" />
    <aura:attribute name="CustomerTransportationValue" type="List" default="" />
    <aura:attribute name="SupplierTransportationValue" type="List" default="" />

    <aura:attribute name="isAccount" type="Boolean" default="true" />
    <aura:attribute name="isTRCR" type="Boolean" default="false" />
    <aura:attribute name="isCustomerProductSelected" type="Boolean" default="false" />
    <aura:attribute name="isSupplierProductSelected" type="Boolean" default="false" />
    <aura:attribute name="isNoCustomerProducts" type="Boolean" default="true" />

    <aura:attribute name="isPetroleum" type="Boolean" default="false" />
    <aura:attribute name="isLube" type="Boolean" default="false" />
    <aura:attribute name="isLABIX" type="Boolean" default="false" />
    <aura:attribute name="isCustomerOtherProducts" type="Boolean" default="false" />
    <aura:attribute name="isSupplierOtherProducts" type="Boolean" default="false" />
    <aura:attribute name="isSameAddress" type="Boolean" default="false" />
    <aura:attribute name="isInitial" type="Boolean" default="false" />

    <aura:attribute name="isOtherTypeofBussiness" type="Boolean" default="false" />
    <aura:attribute name="isOtherLegalEntity" type="Boolean" default="false" />

    <aura:attribute name="isHide" type="Boolean" default="true" />
    <aura:attribute name="ObjectType" type="String" default="Account" />
    <aura:attribute name="onPageRecordID" type="String" />
    <aura:attribute name="isCustomerType" type="Boolean" default="true" />
    <aura:attribute name="isSupplierType" type="Boolean" default="true" />
    <aura:attribute name="IsPageDisable" type="Boolean" default="false" />
    <aura:attribute name="IsSaved" type="Boolean" default="false" />
    <aura:attribute name="IsCounterParty" type="Boolean" default="false" />


    <aura:attribute name="YesPaymentOverTheAmountOfUSD" type="Boolean" default="false" />
    <aura:attribute name="NoPaymentOverTheAmountOfUSD" type="Boolean" default="false" />
    <aura:attribute name="yes4" type="Boolean" default="false" />
    <aura:attribute name="no4" type="Boolean" default="false" />
    <aura:attribute name="YesEnvironmentPolicy" type="Boolean" default="false" />
    <aura:attribute name="NoEnvironmentPolicy" type="Boolean" default="false" />
    <aura:attribute name="YesOccupationalHealthAndSafetyPolicy" type="Boolean" default="false" />
    <aura:attribute name="NoOccupationalHealthAndSafetyPolicy" type="Boolean" default="false" />
    <aura:attribute name="YesCorporateGovernancePolicy" type="Boolean" default="false" />
    <aura:attribute name="NoCorporateGovernancePolicy" type="Boolean" default="false" />
    <aura:attribute name="YesSocialResponsibilityPolicy" type="Boolean" default="false" />
    <aura:attribute name="NoSocialResponsibilityPolicy" type="Boolean" default="false" />
    <aura:attribute name="YesHumanRightsPolicy" type="Boolean" default="false" />
    <aura:attribute name="NoHumanRightsPolicy" type="Boolean" default="false" />
    <aura:attribute name="YesHasIssuesDuringThePast5Years" type="Boolean" default="false" />
    <aura:attribute name="NoHasIssuesDuringThePast5Years" type="Boolean" default="false" />
    <aura:attribute name="YesHasFailedSupplyInLast3Years" type="Boolean" default="false" />
    <aura:attribute name="NoHasFailedSupplyInLast3Years" type="Boolean" default="false" />
    <aura:attribute name="YesHasSanctionedTarget" type="Boolean" default="false" />
    <aura:attribute name="NoHasSanctionedTarget" type="Boolean" default="false" />

    <aura:attribute name="DisplayReasonPaymentOverTheAmountOfUSD" type="Boolean" default="false" />
    <aura:attribute name="DisplayReasonHasIssuesDuringThePast5Years" type="Boolean" default="false" />
    <aura:attribute name="DisplayHasFailedSupplyInLast3Years" type="Boolean" default="false" />
    <aura:attribute name="DisplayReasonHasSanctionedTarget" type="Boolean" default="false" />

    <aura:attribute name="isNoCustomerProductsSAPSync" type="Boolean" default="true" />
    <aura:attribute name="SAPSync" type="Boolean" default="false" />
    <aura:attribute name="SAPSyncCustomer" type="Boolean" default="false" />
    <aura:attribute name="SAPSyncSupplier" type="Boolean" default="false" />

    <aura:attribute name="reqitemId" type="String" />
    <aura:attribute name="accId" type="String" />
    <aura:attribute name="isLoaded" type="Boolean" default="false" />
    <aura:attribute name="isOtherMainProduct" type="Boolean" default="false" />
    <aura:attribute name="isOtherMainService" type="Boolean" default="false" />
    <aura:attribute name="isAccountGroup" type="Boolean" default="false" />
    <aura:attribute name="isDisplay" type="Boolean" default="false" />
    <aura:attribute name="isEdit" type="Boolean" default="false" />
    <aura:attribute name="isModalSensitiveOpen" type="Boolean" default="false" />


    <aura:renderIf isTrue="{!v.isInitial}">
        <!--
        <div role="dialog" tabindex="-1" class="slds-modal slds-modal_small slds-fade-in-open" aria-modal="true" aria-labelledby="modal-heading" aria-describedby="modal-content">
            <div class="slds-modal__container">
                <div class="slds-modal__header slds-p-around_medium">
                    <div class="slds-media slds-media_center">
                        
                        <div class="slds-media__body" id="modal-heading">
                            <p class="slds-text-align_center slds-text-heading_medium">
                                Create Initial Customer
                            </p>
                        </div>
                    </div>
                </div>
                <div class="slds-modal__content slds-p-around_medium" id="modal-content">
                    -->
        <lightning:spinner aura:id="spinner" variant="brand" />

        <div class="card">
            <lightning:layoutItem padding="around-small">
                <lightning:recordEditForm aura:id="recordEditForm" recordId="{!v.onPageRecordID}"
                    objectApiName="{!v.ObjectType}" recordTypeId="{!v.recordTypeId}" onsubmit="{!c.handleSubmit}"
                    onsuccess="{!c.handleSuccess}" onload="{!c.handleLoad}">
                    <div class="slds-section slds-is-open animate" aura:id="CompanyInformation">
                        <!--section header-->
                        <h3 class="slds-section__title">
                            <a aria-controls="CompanyInformationList" class="slds-button slds-section__title-action"
                                onclick="{!c.toggleSection}" data-auraId="CompanyInformation">
                                <lightning:icon iconName="utility:switch" size="x-small"
                                    class="slds-section__title-action-icon slds-button__icon_left"
                                    alternativeText="button icon" />
                                <span class="slds-truncate" title="Counterparty Information">Company Information</span>
                            </a>
                        </h3>
                        <!--section body-->
                        <div class="slds-section__content" id="CompanyInformationList">
                            <!--<lightning:messages />-->
                            <div class="slds-grid slds-wrap">

                                <div class="slds-col slds-size_1-of-4">
                                    <!-- Your lightning:inputField components here LEFT Col -->
                                    <aura:if isTrue="{!!v.isAccount}">
                                        <lightning:inputField aura:id="ConfirmedCounterParty__c"
                                            fieldName="ConfirmedCounterParty__c" variant="label-hidden"
                                            class="visibility: hidden;" />
                                    </aura:if>
                                    <label><span style="color:red; font-style: italic;">* </span>Company Name (English)</label>
                                    <lightning:inputField aura:id="CompanyNameENF__c" fieldName="CompanyNameENF__c"
                                        variant="label-hidden" id="Company Name (English)"
                                        onchange="{!c.handleAddJson}" disabled="{!v.SAPSync}"
                                        required="true"/>
                                    <br />
                                </div>
                                <div class="slds-col slds-size_1-of-4"></div>
                                <div class="slds-col slds-size_1-of-4">
                                    <label><span style="color:red; font-style: italic;">* </span>Type of Business (Branch)</label>
                                    <lightning:inputField aura:id="TypeOfBusinessBranch__c"
                                        fieldName="TypeOfBusinessBranch__c" variant="label-hidden"
                                        id="Type of Business (Branch)" 
                                        onchange="{!c.handleAddJson}" disabled="{!v.SAPSync}"
                                        required="true"/>
                                    <br />
                                </div>
                                
                                <div class="slds-col slds-size_1-of-4"></div>

                                <div class="slds-col slds-size_1-of-4">
                                    <!-- Your lightning:inputField components here LEFT Col -->
                                    <label>Company Name (Other Languages)</label>
                                    <lightning:inputField aura:id="CompanyNameLocal__c" fieldName="CompanyNameLocal__c"
                                        variant="label-hidden" 
                                        id="Company Name (Other Languages)"
                                        onchange="{!c.handleAddJson}" disabled="{!v.SAPSync}"/>
                                    <br />
                                    <label>The Former Name of Company (if any)</label>
                                    <lightning:inputField aura:id="FormerNameofCompany__c"
                                        fieldName="FormerNameofCompany__c" variant="label-hidden"
                                        id="The Former Name of Company (if any)" 
                                        onchange="{!c.handleAddJson}"/>
                                    <br />
                                </div>
                                
                                <div class="slds-col slds-size_1-of-4"> </div>
                                
                                <div class="slds-col slds-size_1-of-4">
                                    <!-- Your lightning:inputField components here Right Col -->
                                    <label>Specify Languages of Company Name</label>
                                    <lightning:inputField aura:id="SpecifyLanguagesOfCompanyName__c"
                                        fieldName="SpecifyLanguagesOfCompanyName__c" variant="label-hidden"
                                        id="Specify Languages of Company Name" 
                                        onchange="{!c.handleAddJson}" disabled="{!v.SAPSync}"/>

                                    <br />
                                    <label for="Country"><span style="color:red; font-style: italic;">*
                                        </span>Country</label>
                                    <lightning:inputField aura:id="Country__c" fieldName="Country__c"
                                        variant="label-hidden" required="true" 
                                        id="Country"
                                        onchange="{!c.handleAddJson}" disabled="{!v.SAPSync}"/>
                                    <br />
                                </div>
                                <div class="slds-col slds-size_1-of-4"></div>

                                <div class="slds-col slds-size_1-of-4">
                                    <!-- Your lightning:inputField components here LEFT Col -->
                                    <label><span style="color:red; font-style: italic;">* </span>Type of Business (Commercial Evalution)</label>
                                    <lightning:inputField aura:id="TypeOfBusinessCommercialEvaluation__c"
                                        fieldName="TypeOfBusinessCommercialEvaluation__c" variant="label-hidden"
                                        required="true" 
                                        id="Type of Bussiness (Commercial Evaluation)"
                                        onchange="{!c.handleTypeofBussinessChange}"/>
                                    <br />
                                    <label><span style="color:red; font-style: italic;">* </span>Legal Entity (Commercial Evalution)</label>
                                    <lightning:inputField aura:id="LegalEntity__c" fieldName="LegalEntity__c"
                                        variant="label-hidden" required="true"
                                        id="Legal Entity (Commercial Evaluation)" 
                                        onchange="{!c.handleisOtherLegalEntityChange}"/>
                                    <br />
                                    <label><span style="color:red; font-style: italic;">* </span>Incoperation Date</label>
                                    <lightning:inputField aura:id="IncoperationDate__c" fieldName="IncoperationDate__c"
                                        variant="label-hidden" required="true" 
                                        id="Incoperation Date"
                                        onchange="{!c.handleAddJson}" disabled="{!v.SAPSync}"/>
                                    <br />
                                    <label class="slds-form-element__label">
                                        <abbr class="slds-required" title="required">*</abbr>
                                        <span class="slds-m-right_x-small">Tax Number</span>
                                        <lightning:helptext
                                            content="N = number / A = letter / X = A, B, C, F, G, H, L, J, P, T or K / Y = letter or number / Z = Z / Thailand: NNNNNNNNNNNNN / India: NNAXXXANNNNAYZY /  Vietnam: NNNNNNNNNN / Indonesia: NNNNNNNNNNNNNNN / Singapore: NNNNNNNNNA" />
                                    </label>
                                    <lightning:inputField aura:id="TaxCode1__c" fieldName="TaxCode1__c"
                                        variant="label-hidden" required="true" 
                                        id="Tax Number"
                                        onchange="{!c.handleAddJson}" disabled="{!v.SAPSync}"/>
                                    <br />
                                </div>
                                <div class="slds-col slds-size_1-of-4"></div>

                                
                                
                                <div class="slds-col slds-size_1-of-4">
                                    <!-- Your lightning:inputField components here Right Col -->
                                    <span>
                                        <aura:renderIf isTrue="{!v.isOtherTypeofBussiness}">
                                            <span style="color:red; font-style: italic;">* </span>
                                        </aura:renderIf>
                                        Other Type of Bussiness (Please Specify)
                                    </span>
                                    <lightning:inputField aura:id="TypeOfBusinessOthers__c"
                                        fieldName="TypeOfBusinessOthers__c" variant="label-hidden"
                                        id="Other Type of Bussiness (Please Specify)" 
                                        onchange="{!c.handleAddJson}"
                                        required="{!v.isOtherTypeofBussiness}"/>
                                    <br />
                                    <span>
                                        <aura:renderIf isTrue="{!v.isOtherLegalEntity}">
                                            <span style="color:red; font-style: italic;">* </span>
                                        </aura:renderIf>
                                        Other Legal Entity (Please Specify)
                                    </span>
                                    <lightning:inputField aura:id="LegalEntityOthers__c"
                                        fieldName="LegalEntityOthers__c" variant="label-hidden"
                                        id="Other Legal Entity (Please Specify)" 
                                        onchange="{!c.handleAddJson}"
                                        required="{!v.isOtherLegalEntity}"/>
                                    <br />
                                    <label><span style="color:red; font-style: italic;">* </span>Years of Experience in business?</label>
                                    <lightning:inputField aura:id="Yearofexperience__c" fieldName="Yearofexperience__c"
                                        variant="label-hidden" required="true" 
                                        id="Year of experience in business"
                                        onchange="{!c.handleAddJson}" disabled="{!v.SAPSync}"/>
                                    <br />
                                    <label>Company Website</label>
                                    <aura:renderIf isTrue="{!v.isAccount}">
                                        <lightning:inputField aura:id="Website" fieldName="Website"
                                            variant="label-hidden" 
                                            id="Company Website" 
                                            onchange="{!c.handleAddJson}"/>
                                    </aura:renderIf>
                                    <aura:renderIf isTrue="{!!v.isAccount}">
                                        <lightning:inputField aura:id="Website" fieldName="CompanyWebsite__c"
                                            variant="label-hidden" 
                                            id="Company Website" 
                                            onchange="{!c.handleAddJson}"/>
                                    </aura:renderIf>
                                    <br />
                                </div>
                                <div class="slds-col slds-size_1-of-4"></div>

                                <!--<div class="slds-col slds-size_3-of-4">
                                                  
                                                <label>For Domestic Petroleum Customer, please specify license number of Registration issues by Ministry of Energy</label><br/>
                                                <lightning:inputField aura:id="NoOfRegistrationMinistryEnergy__c" fieldName="NoOfRegistrationMinistryEnergy__c" variant="label-hidden" />
                                                <br/>
                                            </div>
                                            <div class="slds-col slds-size_1-of-4"></div>-->



                                <div class="slds-col slds-size_1-of-4">
                                    <!-- Your lightning:inputField components here LEFT Col -->
                                    <label><span style="color:red; font-style: italic;">* </span>CEO/President/MD First Name</label>
                                    <lightning:inputField aura:id="CEOFirstName__c" fieldName="CEOFirstName__c"
                                        variant="label-hidden" required="true"
                                        id="CEO/President/Managing Director First Name" 
                                        onchange="{!c.handleAddJson}"/>
                                    <br />
                                </div>
                                <div class="slds-col slds-size_1-of-4"></div>
                                <div class="slds-col slds-size_1-of-4">
                                    <!-- Your lightning:inputField components here Right Col -->
                                    <label><span style="color:red; font-style: italic;">* </span>CEO/President/MD Last Name</label>
                                    <lightning:inputField aura:id="CEOLastName__c" fieldName="CEOLastName__c"
                                        variant="label-hidden" required="true"
                                        id="CEO/President/Managing Director Last Name" 
                                        onchange="{!c.handleAddJson}"/>
                                    <br />
                                </div>
                                <div class="slds-col slds-size_1-of-4"></div>

                            <aura:if isTrue="{!v.isDisplay}">
                                <div class="slds-col slds-size_1-of-1">
                                    <p><span style="color:red; font-style: italic;">* </span>List of Shareholders who hold an interst in the company more than 10%</p>
                                </div>
                                <div class="slds-col slds-size_1-of-1 slds-align_absolute-center">
                                    <div class="slds-col slds-size_1-of-1">
                                        <table class="slds-table slds-table_cell-buffer slds-table_bordered"
                                            aria-labelledby="element-with-table-label other-element-with-table-label">
                                            <thead>
                                                <tr class="slds-line-height_reset">

                                                    <!-- <th class="" scope="col">
                                                                        <div class="slds-truncate" title="Opportunity Name">No</div>
                                                                    </th> -->
                                                    <th class="" scope="col">
                                                        <div class="slds-truncate" title="Account Name">Name</div>
                                                    </th>
                                                    <th class="" scope="col">
                                                        <div class="slds-truncate" title="Stage">% of Shareholders</div>
                                                    </th>
                                                    <aura:if isTrue="{!!v.isEdit}">
                                                        <th scope="col" class="">
                                                            <a onclick="{!c.addNewRow}">
                                                                <lightning:icon iconName="utility:add"
                                                                    class="slds-icon slds-icon_small" size="small"
                                                                    alternativeText="add" />
                                                                <span class="slds-assistive-text">Add Icon</span>
                                                            </a>
                                                        </th>
                                                    </aura:if>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <aura:iteration items="{!v.shareholderList}" var="item"
                                                    indexVar="index">
                                                    <c:CTRDynamicRowTable rowIndex="{!index}" ShareHolder="{!item}"
                                                        rowcolumn="false" isDisabled="{!v.isEdit}"/>
                                                </aura:iteration>

                                            </tbody>
                                        </table>
                                        <br />
                                    </div>
                                    <br /><br />
                                </div>
                            </aura:if>

                                <div class="slds-col slds-size_1-of-1">
                                    <p>Parent Company:(If any) (Please give full legal/official company name)</p> <br />
                                </div>

                                <div class="slds-col slds-size_1-of-4">
                                    <label for="ImmidiateParent">Immidiate Parent Company</label>
                                    <!-- Your lightning:inputField components here LEFT Col -->
                                    <lightning:inputField aura:id="ImmidiateParentCompany__c"
                                        fieldName="ImmidiateParentCompany__c" variant="label-hidden"
                                        id="Immidiate Parent Company" 
                                        onchange="{!c.handleAddJson}"/>
                                    <br />
                                </div>
                                <div class="slds-col slds-size_3-of-4"></div>

                                <div class="slds-col slds-size_1-of-4">
                                    <!-- Your lightning:inputField components here LEFT Col -->
                                    <aura:renderIf isTrue="{!v.isCustomerType}">
                                        <label>Customer Search Term (EN)</label>
                                        <lightning:inputField aura:id="CustomerSearchTermEN__c"
                                            fieldName="CustomerSearchTermEN__c" variant="label-hidden"
                                            id="Customer Search Term (EN)" 
                                            onchange="{!c.handleAddJson}" disabled="{!v.SAPSyncCustomer}"/>
                                        <br />
                                    </aura:renderIf>

                                    <aura:renderIf isTrue="{!v.isCustomerType}">
                                        <label class="slds-form-element__label">
                                            <span>Customer Search Term (Other Languages)</span>
                                            <lightning:helptext content="Thai, Bahasa, or English launguage" />
                                        </label>
                                        <lightning:inputField aura:id="CustomerSearchTermLocal__c"
                                            fieldName="CustomerSearchTermLocal__c" variant="label-hidden"
                                            id="Customer Search Term (Other Languages)" 
                                            onchange="{!c.handleAddJson}" disabled="{!v.SAPSyncCustomer}"/>
                                        <br />
                                    </aura:renderIf>
                                </div>

                                <div class="slds-col slds-size_1-of-4"></div>

                                <div class="slds-col slds-size_1-of-4">
                                    <!-- Your lightning:inputField components here Right Col -->
                                    <aura:renderIf isTrue="{!v.isSupplierType}">
                                        <label>Supplier Search Term (EN)</label>
                                        <lightning:inputField aura:id="SupplierSearchTermEN__c"
                                            fieldName="SupplierSearchTermEN__c" variant="label-hidden"
                                            id="Supplier Search Term (EN)" 
                                            onchange="{!c.handleAddJson}" disabled="{!v.SAPSyncSupplier}"/>
                                        <br />
                                    </aura:renderIf>

                                    <aura:renderIf isTrue="{!v.isSupplierType}">
                                        <label class="slds-form-element__label">
                                            <span>Supplier Search Term (Other Languages)</span>
                                            <lightning:helptext content="Thai, Bahasa, or English launguage" />
                                        </label>
                                        <lightning:inputField aura:id="SupplierSearchTermLocal__c"
                                            fieldName="SupplierSearchTermLocal__c" variant="label-hidden"
                                            id="Supplier Search Term (Other Languages)" 
                                            onchange="{!c.handleAddJson}" disabled="{!v.SAPSyncSupplier}"/>
                                        <br />
                                    </aura:renderIf>
                                </div>
                                <div class="slds-col slds-size_1-of-4"></div>
                            </div>
                        </div>
                    </div>

                    <div class="slds-section slds-is-open animate" aura:id="RegisteredAddress1">
                        <!--section header-->
                        <h3 class="slds-section__title">
                            <a aria-controls="RegisteredAddressList" class="slds-button slds-section__title-action"
                                onclick="{!c.toggleSection}" data-auraId="RegisteredAddress1">
                                <lightning:icon iconName="utility:switch" size="x-small"
                                    class="slds-section__title-action-icon slds-button__icon_left"
                                    alternativeText="button icon" />
                                <span class="slds-truncate" title="Counterparty Information">Registered Address</span>
                            </a>
                        </h3>
                        <!--section body-->
                        <div class="slds-section__content" id="RegisteredAddressList">
                            <div class="slds-grid slds-wrap">

                                <div class="slds-col slds-size_1-of-4">
                                    <!-- Your lightning:inputField components here LEFT Col -->
                                    <label>Building Code</label>
                                    <lightning:inputField aura:id="BuildingCodeLocal__c"
                                        fieldName="BuildingCodeLocal__c" variant="label-hidden" 
                                        id="Building Code"
                                        onchange="{!c.handleAddJson}" disabled="{!v.SAPSync}"/>
                                    <br />
                                    <label>Floor</label>
                                    <lightning:inputField aura:id="FloorLocal__c" fieldName="FloorLocal__c"
                                        variant="label-hidden" 
                                        id="Floor" 
                                        onchange="{!c.handleAddJson}" disabled="{!v.SAPSync}"/>
                                    <br />
                                    <label for="House"><span style="color:red; font-style: italic;">* </span>House no.</label>
                                    <lightning:inputField aura:id="HousenoLocal__c" fieldName="HousenoLocal__c"
