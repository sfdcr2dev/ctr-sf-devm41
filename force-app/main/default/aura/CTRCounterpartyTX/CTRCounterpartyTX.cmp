    <aura:attribute name="ObjectType" type="String" default="Account"/>
    <aura:attribute name="isEdit" type="Boolean" default="false"/>
    <aura:attribute name="isLoaded" type="Boolean" default="false" />
    <aura:attribute name="isAccount" type="Boolean" default="true"/>
    <aura:attribute name="isCustomerType" type="Boolean" default="true"/>
    <aura:attribute name="isSupplierType" type="Boolean" default="true"/>
    <aura:attribute name="isTX" type="Boolean" default="true"/>

    
