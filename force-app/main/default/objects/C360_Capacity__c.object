    </validationRules>
    <validationRules>
        <fullName>CheckSalesOrg</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
    AND(
        ISPICKVAL(SalesOrganization__c,&quot;1100&quot;),
        NOT(INCLUDES($User.SalesOrganization__c,&quot;1100&quot;))
    ),
    AND(
        ISPICKVAL(SalesOrganization__c,&quot;1300&quot;),
        NOT(INCLUDES($User.SalesOrganization__c,&quot;1300&quot;))
    ),
    AND(
        ISPICKVAL(SalesOrganization__c,&quot;1400&quot;),
        NOT(INCLUDES($User.SalesOrganization__c,&quot;1400&quot;))
    ),
    AND(
        ISPICKVAL(SalesOrganization__c,&quot;1700&quot;),
        NOT(INCLUDES($User.SalesOrganization__c,&quot;1700&quot;))
    ),
    AND(
        ISPICKVAL(SalesOrganization__c,&quot;2000&quot;),
        NOT(INCLUDES($User.SalesOrganization__c,&quot;2000&quot;))
    ),
    AND(
        ISPICKVAL(SalesOrganization__c,&quot;9100&quot;),
        NOT(INCLUDES($User.SalesOrganization__c,&quot;9100&quot;))
    ),
    AND(
        ISPICKVAL(SalesOrganization__c,&quot;9200&quot;),
        NOT(INCLUDES($User.SalesOrganization__c,&quot;9200&quot;))
    ),
    AND(
        ISPICKVAL(SalesOrganization__c,&quot;9300&quot;),
        NOT(INCLUDES($User.SalesOrganization__c,&quot;9300&quot;))
    ),
    AND(
        ISPICKVAL(SalesOrganization__c,&quot;9400&quot;),
        NOT(INCLUDES($User.SalesOrganization__c,&quot;9400&quot;))
    )
)</errorConditionFormula>
        <errorDisplayField>SalesOrganization__c</errorDisplayField>
        <errorMessage>Please recheck sales organization before saving.</errorMessage>
    </validationRules>
