    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Check_AMM_Appoint_Disappoint_File</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Type__c, &quot;AMM&quot;), ISPICKVAL(AMM_Conclusion__c, &quot;Appoint&quot;), Appointed_Disappointed_File__c = false, ISPICKVAL(Status__c, &quot;Waiting Requester Update Result&quot;))</errorConditionFormula>
        <errorDisplayField>Appointed_Disappointed_File__c</errorDisplayField>
        <errorMessage>Appointed/Disappointed File need to be uploaded and checked if AMM Conclusion is &quot;Appoint&quot; .</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_AMM_Circular_File</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Type__c, &quot;AMM&quot;), ISPICKVAL(AMM_Conclusion__c, &quot;Appoint&quot;), Circular_File__c = false, ISPICKVAL(Status__c, &quot;Waiting Requester Update Result&quot;))</errorConditionFormula>
        <errorDisplayField>Circular_File__c</errorDisplayField>
        <errorMessage>Circular File need to be check.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_AMM_Conclusion_Disappoint</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(AMM_Conclusion__c, &quot;Disappoint&quot;),  Appointed_Disappointed_File__c = false)</errorConditionFormula>
        <errorDisplayField>Appointed_Disappointed_File__c</errorDisplayField>
        <errorMessage>Appointed/Disappointed File need to be uploaded and checked if AMM Conclusion is &quot;Disappoint&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_AMM_EVP_Approved</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Type__c, &quot;AMM&quot;), ISPICKVAL(AMM_Conclusion__c, &quot;Appoint&quot;), EVP_Approved__c = false, ISPICKVAL(Status__c, &quot;Waiting Requester Update Result&quot;))</errorConditionFormula>
        <errorDisplayField>EVP_Approved__c</errorDisplayField>
        <errorMessage>EVP Approved need to be checked if AMM Conclusion is &quot;Appoint&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_AMM_Signed_Circular_File</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Type__c, &quot;AMM&quot;), ISPICKVAL(Status__c, &quot;Waiting Signed Circular File&quot;), NOT(ISBLANK(Circular_No__c)), Signed_Circular_File__c = false)</errorConditionFormula>
        <errorDisplayField>Signed_Circular_File__c</errorDisplayField>
        <errorMessage>Signed Circular File need to be check, if Circular No is filled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_CEO_Circular_File</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Type__c, &quot;CEO&quot;),  Circular_File__c = false, ISPICKVAL(Status__c, &quot;Waiting Requester Update Result&quot;))</errorConditionFormula>
        <errorDisplayField>Circular_File__c</errorDisplayField>
        <errorMessage>Circular File need to be check.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_CEO_EVP_Approved</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Type__c, &quot;CEO&quot;), EVP_Approved__c = false, ISPICKVAL(Status__c, &quot;Waiting Requester Update Result&quot;))</errorConditionFormula>
        <errorDisplayField>EVP_Approved__c</errorDisplayField>
        <errorMessage>EVP Approved need to be checked when Status is &quot;Waiting Requester Update Result&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_CEO_Signed_Circular_File</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Type__c, &quot;CEO&quot;), ISPICKVAL(Status__c, &quot;Waiting Signed Circular File&quot;), NOT(ISBLANK(Circular_No__c)), Signed_Circular_File__c = false)</errorConditionFormula>
        <errorDisplayField>Signed_Circular_File__c</errorDisplayField>
        <errorMessage>Signed Circular File need to be check, if status is &apos;Appointed&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Circular_No</fullName>
        <active>true</active>
        <errorConditionFormula>OR
(
AND(ISPICKVAL(Status__c,&apos;Waiting Circular No.&apos;),ISBLANK(Circular_No__c))
,
AND(ISPICKVAL(Status__c,&apos;Waiting Signed Circular File&apos;),ISBLANK(Circular_No__c))
)</errorConditionFormula>
        <errorDisplayField>Circular_No__c</errorDisplayField>
        <errorMessage>Please enter Circular No before Save</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_End_Date_with_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>End_Date__c &lt; Start_Date__c</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>Please check your &quot;End Date&quot;. End Date should not less than Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Type_Reason_Require</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(OR
(ISPICKVAL(Type__c, &apos;Other&apos;), ISPICKVAL(Type__c, &apos;CEO&apos;)) ,ISBLANK(Type_Remark__c) )</errorConditionFormula>
        <errorDisplayField>Type_Remark__c</errorDisplayField>
        <errorMessage>กรุณากรอกหมายเหตุ ถ้าเลือก Type เป็น CEO หรือ อื่นๆ</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
