    <validationRules>
        <fullName>Validate_User_News_Admin_Category</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
	OR(ISNEW(),
		OR(ISCHANGED(Category__c),
			OR(ISCHANGED(Coin__c),
				OR(ISCHANGED(Coin_Expire__c),
					OR(ISCHANGED(Coin_Type__c),
						OR(ISCHANGED(Content__c),
							OR(ISCHANGED(Expire_Date__c),
								OR(ISCHANGED(Highlight__c),
									OR(ISCHANGED(Link__c),
										OR(ISCHANGED(Publish_Date__c),
											OR(ISCHANGED(Status__c),
												OR(ISCHANGED(Sub_Category__c),
													OR(ISCHANGED(Subtitle__c),
														OR(ISCHANGED(Title__c), ISCHANGED(Video_Link__c))
													)
												)
											)
										)
									)
								)
							)
						)
					)
				)
			)
		)
	),
	NOT(
		IF(TEXT(Category__c) == &quot;PM4U&quot; &amp;&amp; INCLUDES($User.OPP_News_Admin_Category__c, &quot;PM4U&quot;), true,
			IF(TEXT(Category__c) == &quot;People&quot; &amp;&amp; INCLUDES($User.OPP_News_Admin_Category__c, &quot;People&quot;), true,
				IF(TEXT(Category__c) == &quot;KM&quot; &amp;&amp; INCLUDES($User.OPP_News_Admin_Category__c, &quot;KM&quot;), true,
					IF(TEXT(Category__c) == &quot;Corporate&quot; &amp;&amp; INCLUDES($User.OPP_News_Admin_Category__c, &quot;Corporate&quot;), true,
						IF(TEXT(Category__c) == &quot;Digital&quot; &amp;&amp; INCLUDES($User.OPP_News_Admin_Category__c, &quot;Digital&quot;), true,
							IF(TEXT(Category__c) == &quot;QSHE&quot; &amp;&amp; INCLUDES($User.OPP_News_Admin_Category__c, &quot;QSHE&quot;), true,
								IF(TEXT(Category__c) == &quot;Club&quot; &amp;&amp; INCLUDES($User.OPP_News_Admin_Category__c, &quot;Club&quot;), true,
									IF(TEXT(Category__c) == &quot;Akanee&quot; &amp;&amp; INCLUDES($User.OPP_News_Admin_Category__c, &quot;Akanee&quot;), true,
										IF(TEXT(Category__c) == &quot;Special Issues&quot; &amp;&amp; INCLUDES($User.OPP_News_Admin_Category__c, &quot;Special Issues&quot;), true, IF(TEXT(Category__c) == &quot;Operation&quot; &amp;&amp; INCLUDES($User.OPP_News_Admin_Category__c, &quot;Operation&quot;), true, false
											)
										)
									)
								)
							)
						)
					)
				)
			)
		)
	)
)</errorConditionFormula>
        <errorMessage>Only News administrator for this category can create news.</errorMessage>
    </validationRules>
